Dialogue: 0,0:00:10.57,0:00:13.93,Loithuat,,0000,0000,0000,,Tiếp chỉ!\NThừa tướng Tào Tháo.
Dialogue: 0,0:00:14.11,0:00:16.35,Loithuat,,0000,0000,0000,,Thống lãnh ba quân.
Dialogue: 0,0:00:16.57,0:00:19.17,Loithuat,,0000,0000,0000,,Bình định Kinh Sở, Ngô Việt
Dialogue: 0,0:00:19.82,0:00:21.02,Loithuat,,0000,0000,0000,,Tạ ơn bệ hạ.
Dialogue: 0,0:00:39.90,0:00:40.78,Loithuat,,0000,0000,0000,,Đây đều là những con dân của đại Hán
Dialogue: 0,0:00:41.98,0:00:44.54,Loithuat,,0000,0000,0000,,Họ không tán thành với cách cai trị\Ncủa Tào Tháo mới đi theo chúng ta
Dialogue: 0,0:00:45.40,0:00:47.04,Loithuat,,0000,0000,0000,,Nếu như ngay cả lão bách tính\Ncòn không bảo vệ nổi,
Dialogue: 0,0:00:47.23,0:00:48.91,Loithuat,,0000,0000,0000,,thử hỏi trận chiến này liệu có ý nghĩa gì?
Dialogue: 0,0:00:52.32,0:00:54.92,Loithuat,,0000,0000,0000,,Hai quân liên minh nhất định\Nsẽ gia tăng sức mạnh.
Dialogue: 0,0:01:02.65,0:01:07.05,Loithuat,,0000,0000,0000,,Một kẻ thất bại cùng một kẻ hèn nhát\Nliên minh với nhau
Dialogue: 0,0:01:07.23,0:01:08.27,Loithuat,,0000,0000,0000,,có thể làm được trò gì đây?
Dialogue: 0,0:01:09.19,0:01:12.43,Loithuat,,0000,0000,0000,,Điều quân đội Đông Ngô cần\Nchính là đồng tâm một lòng.
Dialogue: 0,0:01:17.90,0:01:19.34,Loithuat,,0000,0000,0000,,Ý ta đã quyết
Dialogue: 0,0:01:19.52,0:01:22.08,Loithuat,,0000,0000,0000,,đánh một trận sinh tử với Tào Tháo
Dialogue: 0,0:01:22.32,0:01:25.24,Loithuat,,0000,0000,0000,,Kẻ nào còn nói đầu hàng thì\Nsố phận sẽ như cái bàn này.
Dialogue: 0,0:01:28.98,0:01:30.62,Loithuat,,0000,0000,0000,,Chàng cần phải đánh trận này
Dialogue: 0,0:01:32.77,0:01:34.21,Loithuat,,0000,0000,0000,,Không phải sợ hắn
Dialogue: 0,0:01:35.02,0:01:36.38,Loithuat,,0000,0000,0000,,Tôi đợi tới ngày này từ lâu lắm rồi.
Dialogue: 0,0:01:38.02,0:01:41.42,Loithuat,,0000,0000,0000,,Muốn đánh sẽ đánh,\Nchẳng cần phải nhiều lời
Dialogue: 0,0:01:50.86,0:01:53.30,Loithuat,,0000,0000,0000,,Dựng trại ở đằng kia
Dialogue: 0,0:01:53.86,0:01:55.62,Loithuat,,0000,0000,0000,,ngay đối diện với Xích Bích.
Dialogue: 0,0:01:58.73,0:02:00.53,Loithuat,,0000,0000,0000,,Ngày mai chúng ta có thể sẽ là kẻ địch.
Dialogue: 0,0:02:00.86,0:02:02.42,Loithuat,,0000,0000,0000,,Nếu như thật sự có ngày đó,
Dialogue: 0,0:02:02.61,0:02:04.33,Loithuat,,0000,0000,0000,,Du là Du, Lượng là Lượng.
Dialogue: 0,0:02:06.57,0:02:08.05,Loithuat,,0000,0000,0000,,Tào Tháo không thông hiểu thủy chiến
Dialogue: 0,0:02:08.23,0:02:10.19,Loithuat,,0000,0000,0000,,nhất định sẽ phải dựa vào\NThái Mạo, Trương Doãn.
Dialogue: 0,0:02:10.40,0:02:12.24,Loithuat,,0000,0000,0000,,Nếu như trừ khử được bọn chúng.
Dialogue: 0,0:02:12.86,0:02:15.18,Loithuat,,0000,0000,0000,,Thủy quân của Tào Tháo sẽ chẳng\Ncòn bao nhiêu tác dụng nữa.
Dialogue: 0,0:02:28.26,0:02:35.98,Loithoai,,0000,0000,0000,,{\fad(0,1450)\fs34\pos(641,651)\clip(269,616,288,656)\t(0,1402,\clip(269,616,1004,656)}X Í C H  B Í C H  II
Dialogue: 0,0:02:53.90,0:02:54.14,Loithoai,,0000,0000,0000,,Hay!
Dialogue: 0,0:02:54.15,0:02:55.43,Loithoai,,0000,0000,0000,,Hay!
Dialogue: 0,0:03:06.73,0:03:07.21,Loithoai,,0000,0000,0000,,Hay lắm.
Dialogue: 0,0:03:07.78,0:03:10.38,Loithoai,,0000,0000,0000,,Lũ khỉ con này càng ngày càng nhanh nhẹn.
Dialogue: 0,0:03:10.61,0:03:12.53,Loithoai,,0000,0000,0000,,Từ khi Thừa tướng hạ lệnh\Nluyện tập Cuju (xúc cúc),
Dialogue: 0,0:03:10.61,0:03:15.61,Loitrich,,0000,0000,0000,,(Cuju: một loại bóng đá do người Trung Quốc nghĩ ra được xem như là một bài tập quân sự cho binh lính kể từ thời Hán)
Dialogue: 0,0:03:12.73,0:03:15.61,Loithoai,,0000,0000,0000,,lũ nhóc này càng trở nên dẻo dai, rắn rỏi.
Dialogue: 0,0:03:21.48,0:03:23.44,Loithoai,,0000,0000,0000,,Bám sát vào.
Dialogue: 0,0:03:48.40,0:03:50.24,Loithoai,,0000,0000,0000,,Đưa anh ta vào trong.\NMau lên.
Dialogue: 0,0:04:15.73,0:04:16.57,Loithoai,,0000,0000,0000,,Bám sát vào.
Dialogue: 0,0:04:44.03,0:04:45.03,Loithoai,,0000,0000,0000,,Đói quá là đói.
Dialogue: 0,0:04:48.94,0:04:50.86,Loithoai,,0000,0000,0000,,Hay.
Dialogue: 0,0:04:58.53,0:05:00.05,Loithoai,,0000,0000,0000,,Thế nào?\NKhông tồi đấy chứ?
Dialogue: 0,0:05:07.36,0:05:08.56,Loithoai,,0000,0000,0000,,Chặn hắn lại.
Dialogue: 0,0:05:44.74,0:06:00.14,Loithoai,,0000,0000,0000,,Hay.
Dialogue: 0,0:06:28.57,0:06:30.89,Loithoai,,0000,0000,0000,,Giá cái miệng bình nó to hơn một chút\Ncó phải là hay hơn không.
Dialogue: 0,0:06:31.24,0:06:33.68,Loithoai,,0000,0000,0000,,Miệng bình to đến cỡ nào huynh\Nném cũng không trúng đâu.
Dialogue: 0,0:06:33.94,0:06:37.38,Loithoai,,0000,0000,0000,,Ném đồ vào bình này là trò chơi\Ncủa văn nhân nho sĩ khi đối tửu.
Dialogue: 0,0:06:38.11,0:06:41.51,Loithoai,,0000,0000,0000,,Chúng ta còn đang đánh trận vất vả\Nthì làm gì có thứ hứng thú đó chứ.
Dialogue: 0,0:06:45.07,0:06:47.47,Loithoai,,0000,0000,0000,,Đô đốc, ngài làm thế nào\Nmà ném chuẩn xác vậy.
Dialogue: 0,0:06:47.99,0:06:48.95,Loithoai,,0000,0000,0000,,Chuyên chú.
Dialogue: 0,0:06:49.57,0:06:50.77,Loithoai,,0000,0000,0000,,Khi địch nhân càng ngông cuồng
Dialogue: 0,0:06:51.07,0:06:52.55,Loithoai,,0000,0000,0000,,chúng ta càng cần phải chuyên chú.
Dialogue: 0,0:06:52.94,0:06:56.18,Loithoai,,0000,0000,0000,,Người chuyên chú có thể chỉ dùng một\Nhòn đá nhỏ mà giết được kẻ khổng lồ
Dialogue: 0,0:06:58.86,0:07:01.34,Loithoai,,0000,0000,0000,,Thừa tướng, Chu Du binh ít.
Dialogue: 0,0:07:01.61,0:07:03.97,Loithoai,,0000,0000,0000,,Tấn công thành chỉ có thể đánh lén thôi.
Dialogue: 0,0:07:04.24,0:07:05.48,Loithoai,,0000,0000,0000,,Chu Du tự cho mình làm thanh cao
Dialogue: 0,0:07:05.74,0:07:07.58,Loithoai,,0000,0000,0000,,lại vừa thắng chúng ta một trận\Nnhỏ tại Tam Giang khẩu.
Dialogue: 0,0:07:07.90,0:07:09.74,Loithoai,,0000,0000,0000,,Hắn không phải là kẻ đánh lén đâu
Dialogue: 0,0:07:09.99,0:07:12.63,Loithoai,,0000,0000,0000,,Nhất định sẽ cùng chúng ta\Ngiao chiến trực diện.
Dialogue: 0,0:07:15.90,0:07:17.18,Loithoai,,0000,0000,0000,,Bọn chúng đều đã chuẩn bị\Nsẵn sàng cho trận chiến
Dialogue: 0,0:07:17.40,0:07:20.48,Loithoai,,0000,0000,0000,,Chúng ta cần phải có chiến lược\Nthích hợp đối phó với chúng.
Dialogue: 0,0:07:22.61,0:07:24.09,Loithoai,,0000,0000,0000,,Tào quân muốn bao vây chúng ta
Dialogue: 0,0:07:24.99,0:07:26.75,Loithoai,,0000,0000,0000,,mà lại không vi phạm\Nđạo lý trong binh pháp.
Dialogue: 0,0:07:26.94,0:07:28.94,Loithoai,,0000,0000,0000,,Trước tiên xây dựng trận địa.
Dialogue: 0,0:07:29.24,0:07:31.52,Loithoai,,0000,0000,0000,,Sau đó sẽ tấn công.
Dialogue: 0,0:07:32.03,0:07:33.51,Loithoai,,0000,0000,0000,,Xuất binh lúc địch chưa chuẩn bị.
Dialogue: 0,0:07:33.70,0:07:34.86,Loithoai,,0000,0000,0000,,Tấn công vào nơi chúng không ngờ tới.
Dialogue: 0,0:07:35.03,0:07:36.63,Loithoai,,0000,0000,0000,,Chúng ta phải tập trung toàn bộ lực lượng.
Dialogue: 0,0:07:36.82,0:07:38.54,Loithoai,,0000,0000,0000,,Công kích vào nơi mạnh nhất của bọn chúng.
Dialogue: 0,0:07:40.99,0:07:44.19,Loithoai,,0000,0000,0000,,Thủy quân của chúng ta lần này\Ncó được toàn thắng hay không,
Dialogue: 0,0:07:45.53,0:07:48.85,Loithoai,,0000,0000,0000,,trông cậy cả vào Thái Mạo, Trương Doãn\Nhai vị tướng quân đó.
Dialogue: 0,0:07:49.32,0:07:51.32,Loithoai,,0000,0000,0000,,Tại hạ nguyện tận trung với\Nthừa tướng cho tới chết.
Dialogue: 0,0:07:52.28,0:07:53.36,Loithoai,,0000,0000,0000,,Tào Tháo khinh địch.
Dialogue: 0,0:07:53.70,0:07:56.14,Loithoai,,0000,0000,0000,,chẳng thể nào ngờ đến\Nchúng ta sẽ đánh thủy trại.
Dialogue: 0,0:08:05.07,0:08:06.27,Loithoai,,0000,0000,0000,,Ngươi tên gì?
Dialogue: 0,0:08:06.44,0:08:08.16,Loithoai,,0000,0000,0000,,Tiểu địch tên Tôn Thúc Tài
Dialogue: 0,0:08:08.57,0:08:10.17,Loithoai,,0000,0000,0000,,Xúc cúc của ngươi xuất sắc lắm.
Dialogue: 0,0:08:10.65,0:08:12.29,Loithoai,,0000,0000,0000,,Quả là binh sĩ giỏi của quân ta.
Dialogue: 0,0:08:13.28,0:08:14.68,Loithoai,,0000,0000,0000,,Từ nay trở đi
Dialogue: 0,0:08:15.28,0:08:18.28,Loithoai,,0000,0000,0000,,ngươi được thăng lên làm thiên phu trưởng.
Dialogue: 0,0:08:18.86,0:08:20.30,Loithoai,,0000,0000,0000,,Tạ ơn thừa tướng.
Dialogue: 0,0:08:20.65,0:08:22.53,Loithoai,,0000,0000,0000,,Mọi người cần phải chăm chỉ\Nluyện tập xúc cúc (Cuju).
Dialogue: 0,0:08:23.36,0:08:26.32,Loithoai,,0000,0000,0000,,Chỉ khi thân thể khỏe mạnh rắn rỏi\Nchúng ta mới có thể đánh thắng trận
Dialogue: 0,0:08:26.99,0:08:29.79,Loithoai,,0000,0000,0000,,Đợi đến khi Đông Ngô bị thảo phạt.
Dialogue: 0,0:08:30.36,0:08:34.60,Loithoai,,0000,0000,0000,,Mỗi người các ngươi,\Nmỗi hộ được miễn thuế 3 năm.
Dialogue: 0,0:08:36.28,0:08:38.12,Loithoai,,0000,0000,0000,,Tạ ơn thừa tướng.
Dialogue: 0,0:09:13.11,0:09:14.47,Loithoai,,0000,0000,0000,,Cậu đang làm gì vậy?
Dialogue: 0,0:09:15.65,0:09:16.49,Loithoai,,0000,0000,0000,,Tôi...
Dialogue: 0,0:09:16.99,0:09:18.39,Loithoai,,0000,0000,0000,,phóng sinh.
Dialogue: 0,0:09:19.07,0:09:21.11,Loithoai,,0000,0000,0000,,Thật tốt. Cậu quả là hảo tâm
Dialogue: 0,0:09:29.74,0:09:31.46,Loithoai,,0000,0000,0000,,Ra rồi à?
Dialogue: 0,0:09:33.86,0:09:36.58,Loithoai,,0000,0000,0000,,Nghe giọng cậu, chắc cậu\Nlà người phương Nam rồi.
Dialogue: 0,0:09:37.24,0:09:38.16,Loithoai,,0000,0000,0000,,Đúng rồi.
Dialogue: 0,0:09:39.45,0:09:41.25,Loithoai,,0000,0000,0000,,Người phương Nam nhiệt tình
Dialogue: 0,0:09:41.49,0:09:42.89,Loithoai,,0000,0000,0000,,Người phương Bắc phóng khoáng
Dialogue: 0,0:09:43.20,0:09:44.28,Loithoai,,0000,0000,0000,,Tôi có hơi ngốc nghếch một chút
Dialogue: 0,0:09:44.45,0:09:46.29,Loithoai,,0000,0000,0000,,Từ bé đến lớn họ đều nói như vậy...
Dialogue: 0,0:09:47.49,0:09:49.61,Loithoai,,0000,0000,0000,,Cậu không ngốc, mà là thật thà.
Dialogue: 0,0:09:54.65,0:09:55.69,Loithoai,,0000,0000,0000,,Cậu đá cầu giỏi lắm.
Dialogue: 0,0:09:57.20,0:09:59.12,Loithoai,,0000,0000,0000,,Cám ơn ban nãy cậu dùng đầu\Nđỡ giúp tôi một trái.
Dialogue: 0,0:10:00.45,0:10:01.37,Loithoai,,0000,0000,0000,,Mà cậu tên gì nhỉ?
Dialogue: 0,0:10:01.53,0:10:02.73,Loithoai,,0000,0000,0000,,Tôi tên...
Dialogue: 0,0:10:04.53,0:10:05.77,Loithoai,,0000,0000,0000,,Cậu xem...
Dialogue: 0,0:10:06.07,0:10:09.11,Loithoai,,0000,0000,0000,,Bàn Trư : heo mập.
Dialogue: 0,0:10:06.07,0:10:09.11,Loithoai,,0000,0000,0000,,Mẹ tôi toàn gọi tôi là Bàn Trư.
Dialogue: 0,0:10:09.82,0:10:10.66,Loithoai,,0000,0000,0000,,Bàn Trư
Dialogue: 0,0:10:12.20,0:10:13.72,Loithoai,,0000,0000,0000,,Cậu cũng biết chơi xúc cúc chứ?
Dialogue: 0,0:10:14.53,0:10:16.97,Loithoai,,0000,0000,0000,,Tôi nào có biết. Chỉ biết đá ngựa thôi
Dialogue: 0,0:10:17.78,0:10:20.34,Loithoai,,0000,0000,0000,,Chúc mừng nhé!\NGiờ cậu đã là thiên phu trưởng rồi.
Dialogue: 0,0:10:20.70,0:10:23.30,Loithoai,,0000,0000,0000,,Làm cái chức thiên phu trưởng đó liệu\Ncó thể được ăn nhiều hơn chút không nhỉ?
Dialogue: 0,0:10:24.28,0:10:25.48,Loithoai,,0000,0000,0000,,Được làm quan rồi đó.
Dialogue: 0,0:10:25.65,0:10:27.49,Loithoai,,0000,0000,0000,,Cậu muốn ăn bao nhiêu là sẽ có bấy nhiêu.
Dialogue: 0,0:10:28.78,0:10:30.86,Loithoai,,0000,0000,0000,,Cậu đánh trận chỉ để có cơm ăn thôi sao?
Dialogue: 0,0:10:31.36,0:10:32.68,Loithoai,,0000,0000,0000,,Phạn Đồng: thùng gạo
Dialogue: 0,0:10:31.36,0:10:32.68,Loithoai,,0000,0000,0000,,Mẹ tôi gọi tôi là Phạn Đồng mà.
Dialogue: 0,0:10:32.74,0:10:33.14,Loithoai,,0000,0000,0000,,Nhà tôi nghèo quá không có đủ cái ăn.
Dialogue: 0,0:10:33.32,0:10:35.64,Loithoai,,0000,0000,0000,,Nhà tôi nghèo quá không có đủ cái ăn.
Dialogue: 0,0:10:36.40,0:10:39.04,Loithoai,,0000,0000,0000,,Tôi với cậu, một là Phạn Đồng\Nmột người Bàn Trư
Dialogue: 0,0:10:39.28,0:10:40.52,Loithoai,,0000,0000,0000,,hợp nhau quá còn gì!
Dialogue: 0,0:10:42.45,0:10:45.25,Loithoai,,0000,0000,0000,,Quân sĩ của chúng ta không hợp\Nvới thủy thổ nơi này.
Dialogue: 0,0:10:45.70,0:10:47.90,Loithoai,,0000,0000,0000,,Lại thêm 10 năm chinh chiến ròng rã
Dialogue: 0,0:10:48.20,0:10:49.76,Loithoai,,0000,0000,0000,,chẳng cách nào ngăn chặn\Nđược bệnh tật lây nhiễm.
Dialogue: 0,0:10:49.99,0:10:50.71,Loithoai,,0000,0000,0000,,Vì vậy bệnh thương hàn nhanh chóng...
Dialogue: 0,0:10:50.74,0:10:52.02,Loithoai,,0000,0000,0000,,Vì vậy bệnh thương hàn nhanh chóng...
Dialogue: 0,0:10:52.20,0:10:53.32,Loithoai,,0000,0000,0000,,Có chữa trị được không?
Dialogue: 0,0:10:53.91,0:10:54.95,Loithoai,,0000,0000,0000,,Chữa thì chữa rồi,
Dialogue: 0,0:10:55.32,0:10:57.24,Loithoai,,0000,0000,0000,,nhưng không quá 1, 2 ngày...
Dialogue: 0,0:11:05.86,0:11:06.38,Loithoai,,0000,0000,0000,,Chết bao nhiêu rồi?
Dialogue: 0,0:11:06.40,0:11:07.28,Loithoai,,0000,0000,0000,,Chết bao nhiêu rồi?
Dialogue: 0,0:11:07.99,0:11:09.55,Loithoai,,0000,0000,0000,,Đã hơn trăm người rồi.
Dialogue: 0,0:11:09.95,0:11:11.59,Loithoai,,0000,0000,0000,,Chỉ thêm vài ngày nữa thôi là dịch\Nbùng phát khó có thể ngăn chặn được.
Dialogue: 0,0:11:11.78,0:11:13.58,Loithoai,,0000,0000,0000,,Mau mau đem những\Nngười này đi mai táng.
Dialogue: 0,0:11:14.32,0:11:15.72,Loithoai,,0000,0000,0000,,Không thể chôn. Chỉ được thiêu.
Dialogue: 0,0:11:16.03,0:11:18.63,Loithoai,,0000,0000,0000,,Phải thiêu ngay thì may ra mới\Ncó thể ngăn chặn dịch bệnh lây lan.
Dialogue: 0,0:11:19.66,0:11:21.38,Loithoai,,0000,0000,0000,,Các người mau chuẩn bị\Nđem đống thi thể này đi thiêu.
Dialogue: 0,0:11:21.82,0:11:23.38,Loithoai,,0000,0000,0000,,Khoan đã.
Dialogue: 0,0:11:33.99,0:11:35.67,Loithoai,,0000,0000,0000,,Bệnh dịch hoành hành.
Dialogue: 0,0:11:38.07,0:11:39.83,Loithoai,,0000,0000,0000,,Quả nhiên Tào quân không hợp thủy thổ
Dialogue: 0,0:11:40.36,0:11:41.56,Loithoai,,0000,0000,0000,,Bệnh dịch đã lan tràn rồi.
Dialogue: 0,0:11:41.86,0:11:42.54,Loithoai,,0000,0000,0000,,Hay quá!
Dialogue: 0,0:11:42.70,0:11:46.42,Loithoai,,0000,0000,0000,,Đô đốc, chúng ta hãy nhân cơ hội\Nnày công kích bọn chúng.
Dialogue: 0,0:11:46.78,0:11:48.38,Loithoai,,0000,0000,0000,,Bệnh này chỉ có kỵ binh mắc phải.\NThủy quân vẫn còn rất khỏe mạnh.
Dialogue: 0,0:11:48.57,0:11:49.57,Loithoai,,0000,0000,0000,,Không thể khinh suất manh động được.
Dialogue: 0,0:11:49.74,0:11:52.02,Loithoai,,0000,0000,0000,,Đô đốc, đây chính là cơ hội rất tốt\Nđể chúng ta tiến công đó.
Dialogue: 0,0:11:53.95,0:11:56.03,Loithoai,,0000,0000,0000,,Đây là trận chiến quân tử.
Dialogue: 0,0:11:56.78,0:11:59.18,Loithoai,,0000,0000,0000,,Đánh trận cũng phải đánh\Ncho công bằng một chút.
Dialogue: 0,0:12:30.49,0:12:31.53,Loithoai,,0000,0000,0000,,Thừa tướng.
Dialogue: 0,0:12:33.11,0:12:35.67,Loithoai,,0000,0000,0000,,Như thế này là quá tàn nhẫn rồi.
Dialogue: 0,0:12:37.66,0:12:39.18,Loithoai,,0000,0000,0000,,Tiễn binh lính.
Dialogue: 0,0:13:07.12,0:13:08.48,Loithoai,,0000,0000,0000,,Sao vậy?
Dialogue: 0,0:13:28.49,0:13:29.85,Loithoai,,0000,0000,0000,,Kéo thuyền lại đây.
Dialogue: 0,0:13:32.20,0:13:33.52,Loithoai,,0000,0000,0000,,Mọi người xem. Đây là cái gì?
Dialogue: 0,0:13:33.74,0:13:34.58,Loithoai,,0000,0000,0000,,Kéo qua đây.
Dialogue: 0,0:13:38.49,0:13:39.85,Loithoai,,0000,0000,0000,,Kéo thuyền qua đây.
Dialogue: 0,0:13:40.53,0:13:41.73,Loithoai,,0000,0000,0000,,Đều là người chết cả.
Dialogue: 0,0:13:42.24,0:13:43.56,Loithoai,,0000,0000,0000,,Đều là sĩ binh của Tào Tháo.
Dialogue: 0,0:13:43.74,0:13:44.46,Loithoai,,0000,0000,0000,,Mau đi báo với tướng quân.
Dialogue: 0,0:13:44.62,0:13:45.30,Loithoai,,0000,0000,0000,,Rõ.
Dialogue: 0,0:14:14.28,0:14:16.08,Loithoai,,0000,0000,0000,,Quân giáp và vũ khí đặt tại đây.
Dialogue: 0,0:14:16.74,0:14:18.66,Loithoai,,0000,0000,0000,,Thi thể thì đưa sang bên kia.
Dialogue: 0,0:14:24.45,0:14:25.49,Loithoai,,0000,0000,0000,,Tránh ra.
Dialogue: 0,0:14:39.99,0:14:40.83,Loithoai,,0000,0000,0000,,Không được chạm vào.
Dialogue: 0,0:14:41.45,0:14:43.45,Loithoai,,0000,0000,0000,,Những người này đều bị chết\Ndo nhiễm bệnh thương hàn
Dialogue: 0,0:14:43.66,0:14:45.70,Loithoai,,0000,0000,0000,,Tránh ra!\NSẽ bị truyền nhiễm đấy!
Dialogue: 0,0:14:45.95,0:14:48.03,Loithoai,,0000,0000,0000,,Tránh ra!\NKhông được chạm vào bất kì thứ gì.
Dialogue: 0,0:14:48.28,0:14:49.20,Loithoai,,0000,0000,0000,,Mau cách ly khỏi nơi này.
Dialogue: 0,0:14:49.74,0:14:51.50,Loithoai,,0000,0000,0000,,Toàn bộ sơ tán ngay.
Dialogue: 0,0:14:52.24,0:14:54.52,Loithoai,,0000,0000,0000,,Mau hộ tống lão bách tính rời khỏi đây.
Dialogue: 0,0:14:54.99,0:14:56.35,Loithoai,,0000,0000,0000,,Bọn ta là vì muốn tốt cho mọi người thôi.
Dialogue: 0,0:14:56.41,0:14:56.65,Loithoai,,0000,0000,0000,,Bọn ta là vì muốn tốt cho mọi người thôi.
Dialogue: 0,0:14:56.66,0:14:57.66,Loithoai,,0000,0000,0000,,Nơi này sẽ bị lây nhiễm.
Dialogue: 0,0:14:57.82,0:14:59.22,Loithoai,,0000,0000,0000,,Mau rời khỏi đây ngay.
Dialogue: 0,0:15:21.20,0:15:23.92,Loithoai,,0000,0000,0000,,Tên Tào Tháo này thật là tàn nhẫn.
Dialogue: 0,0:15:24.62,0:15:26.46,Loithoai,,0000,0000,0000,,Những binh sĩ đó đã chạm vào các thi thể.
Dialogue: 0,0:15:26.70,0:15:27.98,Loithoai,,0000,0000,0000,,Sợ rằng sẽ lây lan qua người khác.
Dialogue: 0,0:15:28.32,0:15:30.24,Loithoai,,0000,0000,0000,,Tôi đã cách ly bọn họ rồi.
Dialogue: 0,0:15:32.66,0:15:34.10,Loithoai,,0000,0000,0000,,Thế còn người dân gần đó thì sao?
Dialogue: 0,0:15:34.49,0:15:36.37,Loithoai,,0000,0000,0000,,Tôi sợ rằng có không ít người\Nđã bị lây nhiễm rồi.
Dialogue: 0,0:15:36.74,0:15:39.94,Loithoai,,0000,0000,0000,,Tào Tháo dùng thủ đoạn này\Nđúng là bất nhân bất nghĩa.
Dialogue: 0,0:15:40.66,0:15:42.06,Loithoai,,0000,0000,0000,,Công tâm kế.
Dialogue: 0,0:15:42.24,0:15:44.60,Loithoai,,0000,0000,0000,,điều hắn ta muốn chính là\Nđánh vào tâm lý quân ta.
Dialogue: 0,0:15:46.74,0:15:48.82,Loithoai,,0000,0000,0000,,Những thi thể này nên xử lý thế nào đây?
Dialogue: 0,0:15:49.12,0:15:50.80,Loithoai,,0000,0000,0000,,Đây đều là thi thể của kẻ địch
Dialogue: 0,0:15:50.95,0:15:52.87,Loithoai,,0000,0000,0000,,Quăng chúng xuống sông cho cá ăn đi.
Dialogue: 0,0:15:53.03,0:15:55.43,Loithoai,,0000,0000,0000,,Không được.\NHãy an táng tốt cho bọn họ.
Dialogue: 0,0:15:55.66,0:15:56.78,Loithoai,,0000,0000,0000,,Không thể chôn.
Dialogue: 0,0:15:57.41,0:15:59.25,Loithoai,,0000,0000,0000,,Chỉ có thể thiêu.
Dialogue: 0,0:16:47.62,0:16:49.26,Loithoai,,0000,0000,0000,,Đối tửu đương ca
Dialogue: 0,0:16:49.33,0:16:49.85,Loithoai,,0000,0000,0000,,Đối tửu đương ca
Dialogue: 0,0:16:50.37,0:16:53.41,Loithoai,,0000,0000,0000,,Nhân sinh kỷ hà
Dialogue: 0,0:16:54.99,0:16:57.03,Loithoai,,0000,0000,0000,,như triêu sương
Dialogue: 0,0:16:57.28,0:16:59.68,Loithoai,,0000,0000,0000,,Cứ nhật khổ đa
Dialogue: 0,0:16:59.78,0:17:00.30,Loithoai,,0000,0000,0000,,Cứ nhật khổ đa
Dialogue: 0,0:17:00.33,0:17:00.61,Loithoai,,0000,0000,0000,,Cứ nhật khổ đa
Dialogue: 0,0:17:02.33,0:17:04.13,Loithoai,,0000,0000,0000,,Sầu đương dĩ uyển
Dialogue: 0,0:17:04.37,0:17:06.29,Loithoai,,0000,0000,0000,,Ưu tư nan vong
Dialogue: 0,0:17:06.99,0:17:09.07,Loithoai,,0000,0000,0000,,Hà dĩ giải ưu
Dialogue: 0,0:17:09.58,0:17:12.74,Loithoai,,0000,0000,0000,,Duy hữu đỗ khang
Dialogue: 0,0:17:14.58,0:17:16.58,Loithoai,,0000,0000,0000,,Thanh thanh tử khâm
Dialogue: 0,0:17:16.95,0:17:19.19,Loithoai,,0000,0000,0000,,Du du ngã tâm
Dialogue: 0,0:17:19.83,0:17:21.35,Loithoai,,0000,0000,0000,,Đán vị quân cố
Dialogue: 0,0:17:21.91,0:17:24.19,Loithoai,,0000,0000,0000,,Trầm ngâm chí kim
Dialogue: 0,0:17:24.41,0:17:26.13,Loithoai,,0000,0000,0000,,lộc minh
Dialogue: 0,0:17:26.95,0:17:29.27,Loithoai,,0000,0000,0000,,Thực dã chi bình
Dialogue: 0,0:17:30.37,0:17:32.33,Loithoai,,0000,0000,0000,,Ngã hữu gia tân
Dialogue: 0,0:17:32.74,0:17:35.82,Loithoai,,0000,0000,0000,,Cổ sắt xuy địch
Dialogue: 0,0:17:40.08,0:17:42.88,Loithoai,,0000,0000,0000,,Minh minh như nguyệt
Dialogue: 0,0:17:44.49,0:17:47.33,Loithoai,,0000,0000,0000,,Hà thời khả chuyết
Dialogue: 0,0:17:48.62,0:17:51.34,Loithoai,,0000,0000,0000,,Ưu tòng trung lai
Dialogue: 0,0:17:51.70,0:17:54.90,Loithoai,,0000,0000,0000,,Bất khả đoạn tuyệt
Dialogue: 0,0:17:55.99,0:17:57.59,Loithoai,,0000,0000,0000,,Nguyệt minh tinh hi
Dialogue: 0,0:17:57.78,0:17:59.70,Loithoai,,0000,0000,0000,,Ô thước Nam phi
Dialogue: 0,0:18:00.03,0:18:01.95,Loithoai,,0000,0000,0000,,Nhiễu thụ tam tạp
Dialogue: 0,0:18:02.16,0:18:04.24,Loithoai,,0000,0000,0000,,Hà chi khả y
Dialogue: 0,0:18:04.45,0:18:05.97,Loithoai,,0000,0000,0000,,Sơn bất yếm cao
Dialogue: 0,0:18:06.16,0:18:07.68,Loithoai,,0000,0000,0000,,Thủy bất yếm thâm
Dialogue: 0,0:18:07.87,0:18:09.79,Loithoai,,0000,0000,0000,,Chu công thổ bộ
Dialogue: 0,0:18:10.16,0:18:11.76,Loithoai,,0000,0000,0000,,Thiên hạ
Dialogue: 0,0:18:14.33,0:18:16.17,Loithoai,,0000,0000,0000,,quy tâm
Dialogue: 0,0:18:36.49,0:18:38.77,Loithoai,,0000,0000,0000,,Từ từ thôi, cẩn thận nóng đấy.
Dialogue: 0,0:18:45.12,0:18:46.32,Loithoai,,0000,0000,0000,,Người bệnh ngày càng nhiều
Dialogue: 0,0:18:46.49,0:18:47.33,Loithoai,,0000,0000,0000,,Thuốc của huynh có còn đủ dùng không?
Dialogue: 0,0:18:47.49,0:18:49.09,Loithoai,,0000,0000,0000,,Nơi đây cây quế trên núi không nhiều
Dialogue: 0,0:18:49.66,0:18:52.10,Loithoai,,0000,0000,0000,,Tôi đã sai người đi những\Nnơi rất xa để tìm rồi
Dialogue: 0,0:18:53.08,0:18:56.36,Loithoai,,0000,0000,0000,,Thuốc từ cây quế có thể\Nchữa bệnh thương hàn sao?
Dialogue: 0,0:18:56.62,0:18:58.62,Loithoai,,0000,0000,0000,,Trước mắt có thể xoa dịu bệnh tình
Dialogue: 0,0:18:59.16,0:19:02.08,Loithoai,,0000,0000,0000,,Nhưng để trị khỏi bệnh\Nthì cần loại dược liệu khác
Dialogue: 0,0:19:19.03,0:19:20.19,Loithoai,,0000,0000,0000,,Tôi tới xem sao
Dialogue: 0,0:19:36.16,0:19:38.72,Loithoai,,0000,0000,0000,,Mau! Đưa huynh đệ của\Nchúng ta rời khỏi đây ngay!
Dialogue: 0,0:19:39.62,0:19:40.30,Loithoai,,0000,0000,0000,,Nhanh lên một chút!
Dialogue: 0,0:19:40.95,0:19:42.55,Loithoai,,0000,0000,0000,,Dậy đi dậy đi!\NChúng ta đi thôi!
Dialogue: 0,0:19:43.08,0:19:45.24,Loithoai,,0000,0000,0000,,Mau dừng lại!\NCác người làm gì vậy?
Dialogue: 0,0:19:45.53,0:19:47.97,Loithoai,,0000,0000,0000,,Trương tướng quân,\Nngài làm thế này là sao?
Dialogue: 0,0:19:48.37,0:19:50.53,Loithoai,,0000,0000,0000,,Bọn họ đều là người bệnh.\NKhông thể đi được?
Dialogue: 0,0:19:51.16,0:19:53.16,Loithoai,,0000,0000,0000,,Mau lên một chút.\NĐưa người của chúng ta lên ngựa.
Dialogue: 0,0:19:53.37,0:19:55.45,Loithoai,,0000,0000,0000,,Những người này ngàn lần\Nkhông thể đi được.
Dialogue: 0,0:19:55.83,0:19:57.43,Loithoai,,0000,0000,0000,,Bệnh của các ngươi vẫn chưa khỏi mà.
Dialogue: 0,0:19:59.83,0:20:01.59,Loithoai,,0000,0000,0000,,Các ngài muốn đi sao?
Dialogue: 0,0:20:09.16,0:20:14.28,Loithoai,,0000,0000,0000,,Chu đô đốc, bệnh dịch này nằm\Nngoài tầm kiểm soát của chúng tôi.
Dialogue: 0,0:20:15.20,0:20:17.16,Loithoai,,0000,0000,0000,,Tôi chỉ có mỗi hai vạn binh mã
Dialogue: 0,0:20:17.49,0:20:20.13,Loithoai,,0000,0000,0000,,Tình hình này mà cứ tiếp diễn\Nchỉ e rằng chẳng còn gì cả.
Dialogue: 0,0:20:21.12,0:20:24.12,Loithoai,,0000,0000,0000,,Tôi đành rút lui trước\Nchờ tái lập đại kế vậy
Dialogue: 0,0:20:24.45,0:20:28.05,Loithoai,,0000,0000,0000,,Trước tình hình này mà\Nngài lại rút binh sao?
Dialogue: 0,0:20:28.62,0:20:30.50,Loithoai,,0000,0000,0000,,Chính ngài là người đề xuất\Nmối liên minh này,
Dialogue: 0,0:20:30.83,0:20:33.23,Loithoai,,0000,0000,0000,,giờ sinh tử đến rồi mà giờ\Nngài lại định bỏ đi sao?
Dialogue: 0,0:20:33.49,0:20:34.65,Loithoai,,0000,0000,0000,,Đúng là bội tín bất nghĩa
Dialogue: 0,0:20:34.83,0:20:36.67,Loithoai,,0000,0000,0000,,Ngươi... không thể nói như thế được.
Dialogue: 0,0:20:36.95,0:20:38.23,Loithoai,,0000,0000,0000,,Bọn ta đâu phải hạng người đó.
Dialogue: 0,0:20:38.41,0:20:40.89,Loithoai,,0000,0000,0000,,Ngươi thử nhìn xem huynh đệ của bọn ta\Nbiết bao nhiêu người mắc bệnh kia kìa
Dialogue: 0,0:20:41.12,0:20:43.00,Loithoai,,0000,0000,0000,,Vậy chứ huynh đệ của bọn ta\Nkhông phải là người chắc?
Dialogue: 0,0:20:45.45,0:20:46.13,Loithoai,,0000,0000,0000,,Ích kỷ.
Dialogue: 0,0:20:46.29,0:20:47.33,Loithoai,,0000,0000,0000,,Ngươi nói ai ích kỷ?
Dialogue: 0,0:20:47.49,0:20:47.73,Loithoai,,0000,0000,0000,,-Tam đệ.\N-Trình công!
Dialogue: 0,0:20:47.74,0:20:54.46,Loithoai,,0000,0000,0000,,Chúa công, đại chiến sắp bắt đầu rồi.
Dialogue: 0,0:20:54.87,0:20:55.27,Loithoai,,0000,0000,0000,,Ngài sao có thể...
Dialogue: 0,0:20:55.41,0:20:56.57,Loithoai,,0000,0000,0000,,Khổng Minh
Dialogue: 0,0:21:00.41,0:21:02.37,Loithoai,,0000,0000,0000,,Ta đã trải qua hơn 10 năm thất bại
Dialogue: 0,0:21:02.87,0:21:05.27,Loithoai,,0000,0000,0000,,Chẳng thể nào cam chịu kết cục\Nđó thêm một lần nữa
Dialogue: 0,0:21:05.62,0:21:10.42,Loithoai,,0000,0000,0000,,Lại nói chẳng phải trên chiến trường\Ncũng có câu quân tử phòng thân đó sao?
Dialogue: 0,0:21:11.12,0:21:13.04,Loithoai,,0000,0000,0000,,Chúng ta không thể thất tín\Nvới bên liên minh được.
Dialogue: 0,0:21:13.24,0:21:15.04,Loithoai,,0000,0000,0000,,Ta biết ngài là một con người\Ntràn đầy nhiệt huyết
Dialogue: 0,0:21:15.58,0:21:18.78,Loithoai,,0000,0000,0000,,Nhưng sinh phùng loạn thế,\Nchính nghĩa chẳng thể thành cơm ăn.
Dialogue: 0,0:21:19.12,0:21:22.36,Loithoai,,0000,0000,0000,,Tôi nghĩ chỉ còn cách để\Nngười khác đánh trận này vậy
Dialogue: 0,0:21:24.95,0:21:28.95,Loithoai,,0000,0000,0000,,Ngày sau ngài nhất định\Nsẽ hiểu được đạo lý này
Dialogue: 0,0:21:31.24,0:21:32.64,Loithoai,,0000,0000,0000,,Lưu Dự Châu.
Dialogue: 0,0:21:35.33,0:21:38.49,Loithoai,,0000,0000,0000,,Chu Du này thật vinh hạnh được\Ncùng mọi người kề vai tác chiến
Dialogue: 0,0:21:39.58,0:21:41.70,Loithoai,,0000,0000,0000,,Chúng ta đều có chung một niềm tin
Dialogue: 0,0:21:42.74,0:21:45.14,Loithoai,,0000,0000,0000,,nhưng giờ khó có thể thành hiện thực rồi
Dialogue: 0,0:21:46.20,0:21:47.16,Loithoai,,0000,0000,0000,,Bảo trọng.
Dialogue: 0,0:21:48.62,0:21:49.50,Loithoai,,0000,0000,0000,,Bảo trọng.
Dialogue: 0,0:22:03.41,0:22:04.97,Loithoai,,0000,0000,0000,,Sau này sẽ hội ngộ.
Dialogue: 0,0:22:13.49,0:22:15.05,Loithoai,,0000,0000,0000,,Tại sao ngài không đi?
Dialogue: 0,0:22:18.29,0:22:20.25,Loithoai,,0000,0000,0000,,Làm người cần phải giữ chữ tín.
Dialogue: 0,0:22:20.45,0:22:22.73,Loithoai,,0000,0000,0000,,Tôi đã muốn làm chuyện gì thì\Nnhất định sẽ theo tới cùng.
Dialogue: 0,0:22:24.12,0:22:27.08,Loithoai,,0000,0000,0000,,Ai nói những lời này đều\Nlà những kẻ điên khùng cả.
Dialogue: 0,0:22:28.58,0:22:29.82,Loithoai,,0000,0000,0000,,Vậy thì tất cả chúng ta đều điên mất rồi.
Dialogue: 0,0:22:51.08,0:22:52.44,Loithoai,,0000,0000,0000,,Thừa tướng mời dùng trà.
Dialogue: 0,0:22:59.25,0:23:00.77,Loithoai,,0000,0000,0000,,Ca, thám tử tới báo.
Dialogue: 0,0:23:00.95,0:23:02.35,Loithoai,,0000,0000,0000,,Bên Đông Ngô kia đã bắt đầu loạn rồi.
Dialogue: 0,0:23:02.58,0:23:05.06,Loithoai,,0000,0000,0000,,Bệnh thương hàn đã lan truyền qua sông.
Dialogue: 0,0:23:06.20,0:23:07.08,Loithoai,,0000,0000,0000,,Còn nữa
Dialogue: 0,0:23:07.37,0:23:09.73,Loithoai,,0000,0000,0000,,Lưu Bị đem theo nhân mã của hắn đi rồi.
Dialogue: 0,0:23:11.62,0:23:13.34,Loithoai,,0000,0000,0000,,Liên minh của bọn chúng đã tan dã.
Dialogue: 0,0:23:16.04,0:23:16.72,Loithoai,,0000,0000,0000,,Tốt
Dialogue: 0,0:24:12.91,0:24:14.99,Loithoai,,0000,0000,0000,,Uyển chuyển như gió
Dialogue: 0,0:24:21.91,0:24:24.11,Loithoai,,0000,0000,0000,,Mềm mại như rừng
Dialogue: 0,0:24:35.16,0:24:37.32,Loithoai,,0000,0000,0000,,Dữ dội như lửa
Dialogue: 0,0:24:46.96,0:24:49.72,Loithoai,,0000,0000,0000,,Bất động như núi.
Dialogue: 0,0:25:00.08,0:25:01.56,Loithoai,,0000,0000,0000,,Nàng không thích đánh nhau
Dialogue: 0,0:25:01.79,0:25:03.35,Loithoai,,0000,0000,0000,,sao lại biết trích dẫn binh pháp?
Dialogue: 0,0:25:03.87,0:25:05.83,Loithoai,,0000,0000,0000,,Để biết được chàng đang muốn gì
Dialogue: 0,0:25:06.08,0:25:09.24,Loithoai,,0000,0000,0000,,chỉ còn cách lén xem qua binh thư thôi.
Dialogue: 0,0:25:10.45,0:25:11.45,Loithoai,,0000,0000,0000,,Vạn cuốn binh thư
Dialogue: 0,0:25:11.71,0:25:13.71,Loithoai,,0000,0000,0000,,đều không bằng trà ngon mà nàng pha.
Dialogue: 0,0:25:14.75,0:25:17.07,Loithoai,,0000,0000,0000,,Chỉ cần chàng thích là tốt rồi.
Dialogue: 0,0:25:19.95,0:25:22.43,Loithoai,,0000,0000,0000,,Nếu như chúng ta có thể\Nmời Tào Tháo dùng trà
Dialogue: 0,0:25:22.66,0:25:25.46,Loithoai,,0000,0000,0000,,Không bàn chiến tranh,\Nchỉ thưởng phong nguyệt
Dialogue: 0,0:25:26.00,0:25:27.68,Loithoai,,0000,0000,0000,,Thế chẳng phải tốt biết bao.
Dialogue: 0,0:25:29.00,0:25:31.64,Loithoai,,0000,0000,0000,,Ta nghĩ hẳn ta chẳng phải\Nngười biết thưởng thức trà đâu.
Dialogue: 0,0:25:34.25,0:25:35.93,Loithoai,,0000,0000,0000,,Nhìn chàng dụng kiếm
Dialogue: 0,0:25:36.16,0:25:38.20,Loithoai,,0000,0000,0000,,chắc hẳn đã có cách\Nđối phó với Tào Tháo rồi.
Dialogue: 0,0:25:39.83,0:25:41.75,Loithoai,,0000,0000,0000,,Nan tri như âm
Dialogue: 0,0:25:42.96,0:25:44.64,Loithoai,,0000,0000,0000,,Động như lôi chấn
Dialogue: 0,0:25:48.25,0:25:50.45,Loithoai,,0000,0000,0000,,Đến lúc chúng ta hành động rồi.
Dialogue: 0,0:26:05.29,0:26:07.45,Loithoai,,0000,0000,0000,,Cung nghênh thừa tướng.
Dialogue: 0,0:26:08.25,0:26:11.33,Loithoai,,0000,0000,0000,,Hai vị tướng quân,\Nthủy quân luyện tập ra sao rồi?
Dialogue: 0,0:26:11.79,0:26:13.83,Loithoai,,0000,0000,0000,,Bẩm thừa tướng, luyện tập rất tốt.
Dialogue: 0,0:26:14.33,0:26:17.65,Loithoai,,0000,0000,0000,,Tôi đã dùng xích sắt liên kết chiến\Nthuyền của chúng tôi thành một thể.
Dialogue: 0,0:26:17.91,0:26:20.35,Loithoai,,0000,0000,0000,,Quân ta chủ yếu là binh sĩ\Nphương Bắc không quen thủy tính
Dialogue: 0,0:26:20.62,0:26:24.10,Loithoai,,0000,0000,0000,,Nói như vậy là chiến thuyền của chúng\Nta có thể vững trãi như trên đất liền rồi.
Dialogue: 0,0:26:24.37,0:26:27.89,Loithoai,,0000,0000,0000,,Khi gió thổi tới binh sĩ cũng\Nkhông còn sợ bị say sóng nữa.
Dialogue: 0,0:26:28.50,0:26:29.38,Loithoai,,0000,0000,0000,,Có thể tách ra không?
Dialogue: 0,0:26:29.54,0:26:30.34,Loithoai,,0000,0000,0000,,Đương nhiên có thể.
Dialogue: 0,0:26:31.62,0:26:33.62,Loithoai,,0000,0000,0000,,Các huynh đệ, tách thuyền.
Dialogue: 0,0:26:45.91,0:26:48.15,Loithoai,,0000,0000,0000,,Chỉ cần gỡ bỏ những thanh xà ra\Nngay lập tức thuyền sẽ được tách thôi.
Dialogue: 0,0:26:48.37,0:26:51.49,Loithoai,,0000,0000,0000,,Khi đó thuyền trận sẽ\Nchuyển thành "song xoa trận"
Dialogue: 0,0:26:51.83,0:26:52.79,Loithoai,,0000,0000,0000,,trực thủ địch quân.
Dialogue: 0,0:26:53.00,0:26:55.40,Loithoai,,0000,0000,0000,,Xem ra hai vị tướng quân\Nquả là tinh thông thủy chiến,
Dialogue: 0,0:26:55.66,0:26:57.10,Loithoai,,0000,0000,0000,,mới có thể nghĩ ra sách lược này.
Dialogue: 0,0:26:57.41,0:26:58.29,Loithoai,,0000,0000,0000,,Tạ thừa tướng.
Dialogue: 0,0:26:59.54,0:27:00.14,Loithoai,,0000,0000,0000,,Tưởng Can.
Dialogue: 0,0:27:00.33,0:27:01.93,Loithoai,,0000,0000,0000,,Thừa tướng, Tưởng Can đây.
Dialogue: 0,0:27:02.79,0:27:04.59,Loithoai,,0000,0000,0000,,Ngươi và Chu Du cùng lớn lên\Nvới nhau phải không?
Dialogue: 0,0:27:04.83,0:27:07.51,Loithoai,,0000,0000,0000,,Đúng ạ. Tôi và Chu Du\Ntừ nhỏ đã là bạn rất tốt.
Dialogue: 0,0:27:08.00,0:27:09.92,Loithoai,,0000,0000,0000,,Ngươi qua sông thay ta chiêu hàng hắn.
Dialogue: 0,0:27:10.58,0:27:11.86,Loithoai,,0000,0000,0000,,Chiêu hàng, thưa thừa tướng.
Dialogue: 0,0:27:12.16,0:27:15.04,Loithoai,,0000,0000,0000,,Chu Du trọng khí tiết lại rất được\NTôn Quyền trọng dụng.
Dialogue: 0,0:27:15.33,0:27:16.45,Loithoai,,0000,0000,0000,,Chiêu hàng hắn e rằng...
Dialogue: 0,0:27:16.66,0:27:17.78,Loithoai,,0000,0000,0000,,Đông Ngô trên dưới bất hòa.
Dialogue: 0,0:27:17.96,0:27:19.76,Loithoai,,0000,0000,0000,,Lưu Bị cũng đem bộ hạ của hắn đi rồi.
Dialogue: 0,0:27:19.96,0:27:22.32,Loithoai,,0000,0000,0000,,Hiện giờ chính là lúc chiêu hàng Chu Du đó.
Dialogue: 0,0:27:22.54,0:27:23.62,Loithoai,,0000,0000,0000,,Tuân lệnh.
Dialogue: 0,0:27:48.87,0:27:49.59,Loithoai,,0000,0000,0000,,Bàn Trư.
Dialogue: 0,0:27:49.91,0:27:51.99,Loithoai,,0000,0000,0000,,Tôi không làm cậu sợ đây chứ?
Dialogue: 0,0:28:00.16,0:28:01.24,Loithoai,,0000,0000,0000,,Bọn họ tại sao cứ đi theo tôi mãi vậy?
Dialogue: 0,0:28:01.50,0:28:03.78,Loithoai,,0000,0000,0000,,Như thế này làm sao chơi vui được.
Dialogue: 0,0:28:05.46,0:28:08.26,Loithoai,,0000,0000,0000,,Hiện giờ cậu đã là thiên phu trưởng.
Dialogue: 0,0:28:08.50,0:28:10.54,Loithoai,,0000,0000,0000,,Những người đó đương nhiên\Nphải đi theo cậu rồi.
Dialogue: 0,0:28:14.21,0:28:16.97,Loithoai,,0000,0000,0000,,Nghe đây, các người tới\Nthao trường luyện tập đi.
Dialogue: 0,0:28:17.21,0:28:17.77,Loithoai,,0000,0000,0000,,Rõ.
Dialogue: 0,0:28:23.50,0:28:25.02,Loithoai,,0000,0000,0000,,Đúng là tôi nói gì bọn họ cũng nghe thật.
Dialogue: 0,0:28:25.67,0:28:27.03,Loithoai,,0000,0000,0000,,Tôi với cậu đi chơi thôi.
Dialogue: 0,0:28:27.37,0:28:27.85,Loithoai,,0000,0000,0000,,Được.
Dialogue: 0,0:28:31.21,0:28:32.29,Loithoai,,0000,0000,0000,,Cậu muốn chơi gì nào?
Dialogue: 0,0:28:32.46,0:28:33.46,Loithoai,,0000,0000,0000,,Chơi...
Dialogue: 0,0:28:34.42,0:28:35.70,Loithoai,,0000,0000,0000,,Chúng ta chơi công kênh đi!
Dialogue: 0,0:28:37.08,0:28:37.96,Loithoai,,0000,0000,0000,,Nào.
Dialogue: 0,0:28:41.33,0:28:42.29,Loithoai,,0000,0000,0000,,Cậu nặng thật đấy.
Dialogue: 0,0:28:42.46,0:28:44.34,Loithoai,,0000,0000,0000,,Mẹ cậu đặt cho cậu cái\Ntên đó chẳng sai chút nào.
Dialogue: 0,0:28:47.54,0:28:49.70,Loithoai,,0000,0000,0000,,Chỗ này không đủ cao.\NChúng ta lên kia đi.
Dialogue: 0,0:28:49.96,0:28:50.92,Loithoai,,0000,0000,0000,,Ngồi cho vững vào nhé.
Dialogue: 0,0:28:52.42,0:28:53.70,Loithoai,,0000,0000,0000,,Cẩn thận một chút.
Dialogue: 0,0:28:54.17,0:28:55.81,Loithoai,,0000,0000,0000,,Chỗ này đủ cao chưa?
Dialogue: 0,0:28:56.08,0:28:57.20,Loithoai,,0000,0000,0000,,Đủ rồi.
Dialogue: 0,0:28:57.37,0:28:58.45,Loithoai,,0000,0000,0000,,Đừng động đậy nhé!
Dialogue: 0,0:29:06.25,0:29:07.65,Loithoai,,0000,0000,0000,,Ngứa đầu quá.
Dialogue: 0,0:29:07.92,0:29:08.64,Loithoai,,0000,0000,0000,,Ngứa hả?
Dialogue: 0,0:29:09.67,0:29:11.87,Loithoai,,0000,0000,0000,,Vậy để tôi gãi cho.
Dialogue: 0,0:29:12.79,0:29:14.15,Loithoai,,0000,0000,0000,,Tốt hơn rồi.
Dialogue: 0,0:29:17.96,0:29:18.76,Loithoai,,0000,0000,0000,,Sang trái đi.
Dialogue: 0,0:29:18.96,0:29:20.80,Loithoai,,0000,0000,0000,,Phía bên đó đều là thuyền,\Ncó gì hay đâu mà nhìn.
Dialogue: 0,0:29:21.08,0:29:22.12,Loithoai,,0000,0000,0000,,Đừng động đậy.
Dialogue: 0,0:29:27.67,0:29:28.51,Loithoai,,0000,0000,0000,,Chúa công.
Dialogue: 0,0:29:28.83,0:29:32.39,Loithoai,,0000,0000,0000,,Phía địch có gần mười vạn đại quân,\Nhơn 2 ngàn chiến thuyền.
Dialogue: 0,0:29:32.79,0:29:34.87,Loithoai,,0000,0000,0000,,Bên ta thủy lục 2 quân có chưa tới 3 vạn.
Dialogue: 0,0:29:35.29,0:29:37.53,Loithoai,,0000,0000,0000,,Binh lực chênh lệch quá lớn.
Dialogue: 0,0:29:37.87,0:29:40.27,Loithoai,,0000,0000,0000,,Trận này có thể đánh nổi không đây?
Dialogue: 0,0:29:40.50,0:29:42.98,Loithoai,,0000,0000,0000,,Trận này quyết định vận mệnh\NĐông Ngô của chúng ta.
Dialogue: 0,0:29:43.54,0:29:46.18,Loithoai,,0000,0000,0000,,Đánh không nổi cũng phải đánh.
Dialogue: 0,0:29:48.54,0:29:49.50,Loithoai,,0000,0000,0000,,Chúng ta còn lại bao nhiêu mũi tên?
Dialogue: 0,0:29:49.71,0:29:51.87,Loithoai,,0000,0000,0000,,Hiện giờ không đủ 5 vạn.
Dialogue: 0,0:29:52.21,0:29:54.85,Loithoai,,0000,0000,0000,,Khi rút binh Lưu Bị đã mang\Ntheo hơn 4 vạn tên rồi.
Dialogue: 0,0:29:57.08,0:29:59.36,Loithoai,,0000,0000,0000,,Không có số tên này,\Nkhông thể nào tấn công được.
Dialogue: 0,0:29:59.87,0:30:02.75,Loithoai,,0000,0000,0000,,Tên, giờ là trách nhiệm của tôi.
Dialogue: 0,0:30:03.12,0:30:06.72,Loithoai,,0000,0000,0000,,Số tên đó là do chúa công của ngài đem đi,\Nđương nhiên giờ đó là trách nhiệm của ngài rồi.
Dialogue: 0,0:30:07.04,0:30:09.00,Loithoai,,0000,0000,0000,,Hiện giờ còn thiếu 10 vạn tên.
Dialogue: 0,0:30:09.58,0:30:11.62,Loithoai,,0000,0000,0000,,Ngài nghĩ mình làm nổi không?
Dialogue: 0,0:30:12.58,0:30:16.14,Loithoai,,0000,0000,0000,,Được, nếu như tôi không kiếm đủ 10 vạn
Dialogue: 0,0:30:16.62,0:30:18.22,Loithoai,,0000,0000,0000,,y pháp xử trảm.
Dialogue: 0,0:30:19.58,0:30:21.46,Loithoai,,0000,0000,0000,,Quân lương chỉ còn đủ trong 10 ngày
Dialogue: 0,0:30:21.67,0:30:23.35,Loithoai,,0000,0000,0000,,Ta cho ngài thời gian 10 ngày
Dialogue: 0,0:30:24.25,0:30:27.37,Loithoai,,0000,0000,0000,,Không cần tới 10 ngày.\N3 ngày là đủ rồi.
Dialogue: 0,0:30:27.83,0:30:29.99,Loithoai,,0000,0000,0000,,Quân tử nhất ngôn
Dialogue: 0,0:30:31.37,0:30:34.89,Loithoai,,0000,0000,0000,,Ngài từng nói qua sẽ trừ khử\NThái Mạo và Trương Doãn.
Dialogue: 0,0:30:35.42,0:30:37.26,Loithoai,,0000,0000,0000,,Nếu như ngài không làm được thì sao?
Dialogue: 0,0:30:39.92,0:30:41.60,Loithoai,,0000,0000,0000,,Y pháp xử trảm.
Dialogue: 0,0:30:53.75,0:30:57.91,Loithoai,,0000,0000,0000,,Liên khuy thiên hà\NHữu phong như xà.
Dialogue: 0,0:31:06.71,0:31:08.03,Loithoai,,0000,0000,0000,,Ngài đã ở đây một tối rồi.
Dialogue: 0,0:31:08.21,0:31:09.73,Loithoai,,0000,0000,0000,,Thế nào rồi?
Dialogue: 0,0:31:10.58,0:31:12.18,Loithoai,,0000,0000,0000,,Tử Kính huynh, huynh nhìn xem.
Dialogue: 0,0:31:12.46,0:31:14.38,Loithoai,,0000,0000,0000,,Trong đây hội hợp nước\Nvà khí từ tứ phương.
Dialogue: 0,0:31:14.62,0:31:16.02,Loithoai,,0000,0000,0000,,Là nơi phong thủy tốt nhất.
Dialogue: 0,0:31:16.25,0:31:17.57,Loithoai,,0000,0000,0000,,Ngài vẫn còn nhàn nhã quá nhỉ?
Dialogue: 0,0:31:17.79,0:31:19.59,Loithoai,,0000,0000,0000,,Tôi đang lo ngài sắp phải chết đây này
Dialogue: 0,0:31:20.38,0:31:22.18,Loithoai,,0000,0000,0000,,20 thuyền con có thủy quân mà ngài cần
Dialogue: 0,0:31:22.38,0:31:23.90,Loithoai,,0000,0000,0000,,đều đã được chuẩn bị xong xuôi cả rồi.
Dialogue: 0,0:31:30.29,0:31:31.97,Loithoai,,0000,0000,0000,,Hôm nay đã là ngày thứ 2 rồi.
Dialogue: 0,0:31:32.21,0:31:33.29,Loithoai,,0000,0000,0000,,Tên đâu?
Dialogue: 0,0:31:33.50,0:31:34.86,Loithoai,,0000,0000,0000,,Đến 1 mũi tên ngài còn chưa làm được.
Dialogue: 0,0:31:35.08,0:31:36.60,Loithoai,,0000,0000,0000,,Tôi còn đang lo lắng thay ngài\Nđến toát mồ hôi đây này.
Dialogue: 0,0:31:36.88,0:31:38.56,Loithoai,,0000,0000,0000,,Mồ hôi của tôi chắc ngài\Nkhông thấy được đâu.
Dialogue: 0,0:31:38.79,0:31:41.87,Loithoai,,0000,0000,0000,,Nhưng mà thử nhìn xem con rùa\Nđang đổ mồ hôi phải không?
Dialogue: 0,0:31:46.71,0:31:49.19,Loithoai,,0000,0000,0000,,Theo như tôi liệu trước,\Nsắp có sương mù lớn rồi.
Dialogue: 0,0:31:50.17,0:31:51.41,Loithoai,,0000,0000,0000,,Sương mù gì chứ?
Dialogue: 0,0:31:51.58,0:31:53.42,Loithoai,,0000,0000,0000,,Chỉ có đầu tôi đang đầy\Nmột đống sương mù kia kìa.
Dialogue: 0,0:31:54.50,0:31:55.86,Loithoai,,0000,0000,0000,,Chẳng lẽ ngài chưa có dự liệu gì sao?
Dialogue: 0,0:31:56.58,0:31:58.78,Loithoai,,0000,0000,0000,,Thiên cơ bất khả tiết lộ.
Dialogue: 0,0:32:01.17,0:32:05.45,Loithoai,,0000,0000,0000,,Trâu già cày ruộng
Dialogue: 0,0:32:06.42,0:32:10.70,Loithoai,,0000,0000,0000,,Trâu con tóp tép nhai
Dialogue: 0,0:32:11.42,0:32:12.94,Loithoai,,0000,0000,0000,,Huynh quả là một người...
Dialogue: 0,0:32:13.42,0:32:16.54,Loithoai,,0000,0000,0000,,từng đó năm rồi mà vẫn chẳng khác\Ngì một đứa trẻ thích ca hát.
Dialogue: 0,0:32:17.83,0:32:20.87,Loithoai,,0000,0000,0000,,Có ai đoán được đứa trẻ đó bây giờ\Nlại trở thành đại đô đốc cơ chứ.
Dialogue: 0,0:32:22.38,0:32:26.66,Loithoai,,0000,0000,0000,,Cấy cày là việc vặt của lão bá tánh
Dialogue: 0,0:32:27.33,0:32:30.65,Loithoai,,0000,0000,0000,,Năm đó mười tuổi khi huynh từ đằng sau\Nđánh vào đầu tên Hoàng ác bá đó.
Dialogue: 0,0:32:30.92,0:32:33.72,Loithoai,,0000,0000,0000,,Hắn ta quay đầu lại và chẳng hiểu bằng\Ncách nào cây gậy lại ở trong tay tôi.
Dialogue: 0,0:32:34.00,0:32:37.36,Loithoai,,0000,0000,0000,,Hắn liền đánh tôi một trận gần chết.
Dialogue: 0,0:32:38.17,0:32:41.41,Loithoai,,0000,0000,0000,,Thằng bé ngồi ngủ trên chiếc ghế
Dialogue: 0,0:32:45.25,0:32:47.29,Loithoai,,0000,0000,0000,,Đến giờ tôi vẫn không hiểu
Dialogue: 0,0:32:47.58,0:32:49.34,Loithoai,,0000,0000,0000,,tại sao cây gậy đó lại nằm\Ntrong tay tôi được nhỉ?
Dialogue: 0,0:32:49.71,0:32:51.47,Loithoai,,0000,0000,0000,,Tại huynh quá lề mề đó thôi.
Dialogue: 0,0:32:59.25,0:33:00.33,Loithoai,,0000,0000,0000,,Huynh có còn nhớ không
Dialogue: 0,0:33:00.54,0:33:03.74,Loithoai,,0000,0000,0000,,có một lần tôi giả bút tích của huynh\Nviết vào mặt trước cuốn vở
Dialogue: 0,0:33:04.29,0:33:06.65,Loithoai,,0000,0000,0000,,"Già mà không chết thì để làm gì"
Dialogue: 0,0:33:06.92,0:33:07.80,Loithoai,,0000,0000,0000,,Đúng đúng đúng.
Dialogue: 0,0:33:08.38,0:33:11.54,Loithoai,,0000,0000,0000,,Chuyện đó khiến cho lão tiên sinh\Nnổi giận phạt đánh 20 roi vào mông tôi.
Dialogue: 0,0:33:12.00,0:33:13.72,Loithoai,,0000,0000,0000,,Nhớ lại ngày đó
Dialogue: 0,0:33:14.46,0:33:16.06,Loithoai,,0000,0000,0000,,chẳng có trò nghịch gì chúng ta\Nkhông từng thử qua cả.
Dialogue: 0,0:33:16.42,0:33:18.46,Loithoai,,0000,0000,0000,,Với người khác ai tôi cũng có thể lừa được
Dialogue: 0,0:33:18.79,0:33:22.11,Loithoai,,0000,0000,0000,,chứ riêng với huynh, tôi chịu thua.
Dialogue: 0,0:33:24.38,0:33:25.42,Loithoai,,0000,0000,0000,,Mang kiếm tới đây.
Dialogue: 0,0:33:31.63,0:33:32.63,Loithoai,,0000,0000,0000,,Hay.
Dialogue: 0,0:34:01.67,0:34:04.03,Loithoai,,0000,0000,0000,,Huynh múa kiếm đúng là rất tuyệt.
Dialogue: 0,0:34:15.17,0:34:16.97,Loithoai,,0000,0000,0000,,Huynh tới làm thuyết khách\Ncho Tào Tháo phải không?
Dialogue: 0,0:34:19.88,0:34:21.60,Loithoai,,0000,0000,0000,,Đến nói cho bản thân mình\Ntôi còn chẳng làm nổi.
Dialogue: 0,0:34:21.79,0:34:23.23,Loithoai,,0000,0000,0000,,Nào có thể làm thuyết khách gì đó được?
Dialogue: 0,0:34:23.42,0:34:25.22,Loithoai,,0000,0000,0000,,Tôi không thông minh như huynh
Dialogue: 0,0:34:25.46,0:34:27.86,Loithoai,,0000,0000,0000,,nhưng tôi thừa hiểu\Nnhững gì huynh vừa ca.
Dialogue: 0,0:34:28.13,0:34:29.69,Loithoai,,0000,0000,0000,,Huynh xem thường tôi,\Nthật quá đáng lắm rồi đấy
Dialogue: 0,0:34:29.88,0:34:31.72,Loithoai,,0000,0000,0000,,Tôi sao có thể xem thường một\NGiang Nam tài tử như huynh được.
Dialogue: 0,0:34:31.92,0:34:33.64,Loithoai,,0000,0000,0000,,Nhưng huynh quy thuận\NTào Tháo là không đúng.
Dialogue: 0,0:34:34.09,0:34:36.57,Loithoai,,0000,0000,0000,,Chúng ta ai cũng vì chủ của mình,\Nhuynh nói thế...
Dialogue: 0,0:34:36.79,0:34:38.07,Loithoai,,0000,0000,0000,,Không cần nhiều lời.
Dialogue: 0,0:34:38.29,0:34:39.77,Loithoai,,0000,0000,0000,,Chỉ vài ngày nữa thôi,
Dialogue: 0,0:34:40.00,0:34:42.60,Loithoai,,0000,0000,0000,,sẽ có ngay người đem thủ cấp\Ncủa Tào Tháo đặt trước mặt tôi.
Dialogue: 0,0:34:42.84,0:34:44.28,Loithoai,,0000,0000,0000,,Huynh tin không?
Dialogue: 0,0:34:45.79,0:34:46.83,Loithoai,,0000,0000,0000,,Tôi tin.
Dialogue: 0,0:34:47.13,0:34:48.89,Loithoai,,0000,0000,0000,,Đô đốc, có tin cấp báo.
Dialogue: 0,0:34:49.09,0:34:50.89,Loithoai,,0000,0000,0000,,Ngươi không thấy ta đang có khách à?
Dialogue: 0,0:35:03.79,0:35:05.83,Loithoai,,0000,0000,0000,,Tôi biết chúng ta chưa có đủ tên.
Dialogue: 0,0:35:06.04,0:35:07.64,Loithoai,,0000,0000,0000,,Nếu như gã Gia Cát Lượng đó\Nlàm không được như đã nói
Dialogue: 0,0:35:07.92,0:35:08.76,Loithoai,,0000,0000,0000,,y pháp xử trảm.
Dialogue: 0,0:35:08.92,0:35:11.40,Loithoai,,0000,0000,0000,,Bằng hữu của chúng ta ở phía Bắc\Nsông có thể tương trợ không?
Dialogue: 0,0:35:11.63,0:35:14.27,Loithoai,,0000,0000,0000,,Bằng hữu ở phía Bắc sông còn có\Nviệc trọng yếu hơn cần làm.
Dialogue: 0,0:35:14.54,0:35:16.38,Loithoai,,0000,0000,0000,,Thái Mạo và Trương Doãn\N2 vị đô đốc tới báo tin.
Dialogue: 0,0:35:16.59,0:35:18.35,Loithoai,,0000,0000,0000,,Đầu của Tào Tháo trước mắt\Nvẫn chưa thể hạ thủ được.
Dialogue: 0,0:35:18.54,0:35:19.30,Loithoai,,0000,0000,0000,,Chỉ e...
Dialogue: 0,0:35:19.50,0:35:21.02,Loithoai,,0000,0000,0000,,Ngươi đánh giá quá thấp bọn họ rồi.
Dialogue: 0,0:35:21.25,0:35:22.85,Loithoai,,0000,0000,0000,,Phong thư này là do bọn họ gửi tới.
Dialogue: 0,0:35:27.79,0:35:28.79,Loithoai,,0000,0000,0000,,Tốt quá rồi.
Dialogue: 0,0:35:31.54,0:35:32.30,Loithoai,,0000,0000,0000,,Tưởng tiên sinh.
Dialogue: 0,0:35:34.21,0:35:36.13,Loithoai,,0000,0000,0000,,Tại sao ngài lại đứng đây một mình vậy?
Dialogue: 0,0:35:36.71,0:35:37.67,Loithoai,,0000,0000,0000,,Tẩu tẩu phu nhân.
Dialogue: 0,0:35:37.84,0:35:39.08,Loithoai,,0000,0000,0000,,Mau vào trong ngồi đi.
Dialogue: 0,0:35:39.25,0:35:41.25,Loithoai,,0000,0000,0000,,Tiểu Kiều, mang giúp ta\Nvài bình rượu tới đây.
Dialogue: 0,0:35:41.75,0:35:43.35,Loithoai,,0000,0000,0000,,Còn uống nữa sẽ say đấy.
Dialogue: 0,0:35:43.80,0:35:46.20,Loithoai,,0000,0000,0000,,Nào\NChúng ta lên lầu uống rượu thôi.
Dialogue: 0,0:35:47.50,0:35:48.50,Loithoai,,0000,0000,0000,,Còn uống nữa à?
Dialogue: 0,0:35:48.67,0:35:50.07,Loithoai,,0000,0000,0000,,Đang vui mà.
Dialogue: 0,0:35:50.84,0:35:57.00,Loithoai,,0000,0000,0000,,Tôi nghe nói Tào Tháo vì Tẩu Tẩu\Nphu nhân mới binh hạ Giang Nam.
Dialogue: 0,0:35:58.59,0:35:59.35,Loithoai,,0000,0000,0000,,Vậy sao?
Dialogue: 0,0:35:59.50,0:36:03.58,Loithoai,,0000,0000,0000,,Tên Tào Tháo này mang danh\Ncướp vợ của người khác.
Dialogue: 0,0:36:04.38,0:36:05.34,Loithoai,,0000,0000,0000,,Huynh say rồi.
Dialogue: 0,0:36:05.50,0:36:06.22,Loithoai,,0000,0000,0000,,Uống nữa đi.
Dialogue: 0,0:36:06.42,0:36:08.42,Loithoai,,0000,0000,0000,,Uống nữa sao?\NĐi đâu vậy?
Dialogue: 0,0:36:53.04,0:36:54.84,Loithoai,,0000,0000,0000,,Mỗ tôi hàng Tào
Dialogue: 0,0:36:55.05,0:36:57.97,Loithoai,,0000,0000,0000,,không vì lợi riêng mà là vì bị bức bách
Dialogue: 0,0:36:58.21,0:37:01.49,Loithoai,,0000,0000,0000,,Giờ chúng ta đã có người trà trộn\Ntrong doanh trại của quân phương Bắc
Dialogue: 0,0:37:02.09,0:37:06.73,Loithoai,,0000,0000,0000,,có thể thừa cơ thám thính quân tình
Dialogue: 0,0:37:07.55,0:37:09.19,Loithoai,,0000,0000,0000,,đồng thời có thể thừa cơ lấy đầu hắn.
Dialogue: 0,0:37:09.38,0:37:12.46,Loithoai,,0000,0000,0000,,Sớm muộn gì cũng có người tới trình báo.
Dialogue: 0,0:37:12.71,0:37:13.59,Loithoai,,0000,0000,0000,,Thái Mạo.
Dialogue: 0,0:37:13.88,0:37:15.48,Loithoai,,0000,0000,0000,,Đang đọc gì vậy?
Dialogue: 0,0:37:18.21,0:37:20.01,Loithoai,,0000,0000,0000,,Tôi chỉ bày trò đó gạt huynh thôi.
Dialogue: 0,0:37:21.55,0:37:24.07,Loithoai,,0000,0000,0000,,Huynh thật thích lừa gạt người khác.
Dialogue: 0,0:37:25.50,0:37:27.66,Loithoai,,0000,0000,0000,,Tôi lại lừa huynh được một lần nữa rồi...
Dialogue: 0,0:37:29.71,0:37:31.51,Loithoai,,0000,0000,0000,,cả lần sau nữa...
Dialogue: 0,0:37:35.00,0:37:39.68,Loithoai,,0000,0000,0000,,Lần này tôi không để huynh lừa nữa đâu.
Dialogue: 0,0:38:42.67,0:38:46.35,Loithoai,,0000,0000,0000,,Người rơm lão huynh à, huynh so với\Ncái gã Gia Cát lượng này còn tốt hơn nhiều.
Dialogue: 0,0:38:46.63,0:38:48.99,Loithoai,,0000,0000,0000,,Cái gã Gia Cát đó nửa câu\Ncũng chẳng tiết lộ cho tôi.
Dialogue: 0,0:38:49.59,0:38:52.07,Loithoai,,0000,0000,0000,,Chúng ta làm bạn rượu\Nvới nhau rất hợp đấy.
Dialogue: 0,0:38:52.30,0:38:52.94,Loithoai,,0000,0000,0000,,Mời.
Dialogue: 0,0:39:10.67,0:39:11.55,Loithoai,,0000,0000,0000,,Người rơm huynh.
Dialogue: 0,0:39:11.71,0:39:13.79,Loithoai,,0000,0000,0000,,Thuyền tại sao lại thẳng tiến đến\Nchỗ Tào doanh ở Giang Bắc vậy?
Dialogue: 0,0:39:14.01,0:39:16.49,Loithoai,,0000,0000,0000,,Không phải là huynh định\Nquy hàng quân Tào đấy chứ?
Dialogue: 0,0:39:29.92,0:39:32.96,Loithoai,,0000,0000,0000,,Có địch quân!\NCó địch quân tới!
Dialogue: 0,0:39:35.51,0:39:36.63,Loithoai,,0000,0000,0000,,Tách thuyền.
Dialogue: 0,0:39:40.01,0:39:42.49,Loithoai,,0000,0000,0000,,Dẫn chiến thuyền phía trái\Ntiến hành nghênh địch trước.
Dialogue: 0,0:39:52.80,0:39:54.60,Loithoai,,0000,0000,0000,,Mau!\NMau lên!
Dialogue: 0,0:40:04.00,0:40:05.88,Loithoai,,0000,0000,0000,,Người rơm huynh à,\Nđừng có đánh trống nữa.
Dialogue: 0,0:40:06.34,0:40:08.82,Loithoai,,0000,0000,0000,,Nếu như Tào quân tiến đến tiêu diệt\Ncả đám thì biết làm thế nào?
Dialogue: 0,0:40:09.09,0:40:09.93,Loithoai,,0000,0000,0000,,Bọn chúng không dám đầu.
Dialogue: 0,0:40:10.09,0:40:12.29,Loithoai,,0000,0000,0000,,Bọn chúng sợ phục binh\Nsẽ không dám tiếp cận đâu.
Dialogue: 0,0:40:12.51,0:40:14.67,Loithoai,,0000,0000,0000,,Giờ ngài mới chịu nói chuyện với tôi à?
Dialogue: 0,0:40:22.63,0:40:24.15,Loithoai,,0000,0000,0000,,Cung tiễn chuẩn bị!
Dialogue: 0,0:40:24.96,0:40:26.48,Loithoai,,0000,0000,0000,,Cái nào nhìn thấy thì bắn.
Dialogue: 0,0:40:31.13,0:40:31.97,Loithoai,,0000,0000,0000,,Truyền lệnh!
Dialogue: 0,0:40:32.26,0:40:34.62,Loithoai,,0000,0000,0000,,Các thuyền dàn thành hàng.
Dialogue: 0,0:41:04.34,0:41:07.46,Loithoai,,0000,0000,0000,,Đây rõ ràng là kế phản gián.
Dialogue: 0,0:41:11.63,0:41:13.15,Loithoai,,0000,0000,0000,,Chúng ta đâu phải hạng ngốc.
Dialogue: 0,0:41:13.34,0:41:16.98,Loithoai,,0000,0000,0000,,Chu Du lẽ nào lại dùng phương pháp tầm\Nthường thế này để lừa chúng ta sao?
Dialogue: 0,0:41:17.46,0:41:20.06,Loithoai,,0000,0000,0000,,Có những khi phương pháp\Ntưởng chừng giản đơn nhất
Dialogue: 0,0:41:20.34,0:41:22.34,Loithoai,,0000,0000,0000,,lại có thể lừa người ta đến đường chết đấy.
Dialogue: 0,0:41:28.96,0:41:30.32,Loithoai,,0000,0000,0000,,Phóng tiễn.
Dialogue: 0,0:41:47.01,0:41:47.89,Loithoai,,0000,0000,0000,,Phóng tiễn!
Dialogue: 0,0:42:12.92,0:42:15.96,Loithoai,,0000,0000,0000,,Thừa tướng, theo ngài đây\Ncó phải kế phản gián không.
Dialogue: 0,0:42:16.42,0:42:18.50,Loithoai,,0000,0000,0000,,Đều nên cẩn thận vẫn hơn.
Dialogue: 0,0:42:18.76,0:42:20.56,Loithoai,,0000,0000,0000,,Nếu như quân Thái Mạo tạo phản.
Dialogue: 0,0:42:20.76,0:42:22.52,Loithoai,,0000,0000,0000,,Chắc chắn sẽ nguy to.
Dialogue: 0,0:42:23.67,0:42:24.63,Loithoai,,0000,0000,0000,,Phóng tiễn.
Dialogue: 0,0:42:29.42,0:42:30.66,Loithoai,,0000,0000,0000,,Ngoài phong thư này,
Dialogue: 0,0:42:31.01,0:42:33.17,Loithoai,,0000,0000,0000,,tôi còn nghe bọn chúng nói\N...nói cái gì lấy tên
Dialogue: 0,0:42:33.38,0:42:35.54,Loithoai,,0000,0000,0000,,bằng hữu ở bên này của chúng ta\Nsẽ giúp chúng.
Dialogue: 0,0:43:06.55,0:43:07.47,Loithoai,,0000,0000,0000,,Đừng hoảng sợ.
Dialogue: 0,0:43:08.17,0:43:09.81,Loithoai,,0000,0000,0000,,Người rơm kia sẽ giúp ngài tránh tên.
Dialogue: 0,0:43:21.55,0:43:23.15,Loithoai,,0000,0000,0000,,Ngừng đánh trống.
Dialogue: 0,0:43:29.92,0:43:32.48,Loithoai,,0000,0000,0000,,Truyền Thái Mạo,Trương Doãn tới đối chất.
Dialogue: 0,0:43:33.47,0:43:36.35,Loithoai,,0000,0000,0000,,Hiện giờ bọn họ đang\Nluyện binh trên sông.
Dialogue: 0,0:43:36.59,0:43:39.87,Loithoai,,0000,0000,0000,,Người không có, nhưng\Nchữ phải có ở đây chứ?
Dialogue: 0,0:43:40.63,0:43:42.67,Loithoai,,0000,0000,0000,,Mang thư hàng lúc ban đầu\Ncủa chúng tới đây!
Dialogue: 0,0:43:43.05,0:43:45.57,Loithoai,,0000,0000,0000,,Truyền lệnh. Quay thuyền\Nsang hướng bên kia.
Dialogue: 0,0:43:45.84,0:43:46.44,Loithoai,,0000,0000,0000,,Hả?
Dialogue: 0,0:44:03.92,0:44:05.20,Loithoai,,0000,0000,0000,,Các thuyền cùng đánh trống một lúc.
Dialogue: 0,0:44:05.47,0:44:07.03,Loithoai,,0000,0000,0000,,Để tên của bọn chúng biết\Nphương hướng mà bắn.
Dialogue: 0,0:44:12.72,0:44:15.04,Loithoai,,0000,0000,0000,,Hướng về nơi có tiếng trống
Dialogue: 0,0:44:16.34,0:44:16.82,Loithoai,,0000,0000,0000,,Bắn
Dialogue: 0,0:44:21.88,0:44:22.40,Loithoai,,0000,0000,0000,,Nào.\NUống đi.
Dialogue: 0,0:44:22.42,0:44:29.30,Loithoai,,0000,0000,0000,,Ngồi cho vững vào.
Dialogue: 0,0:44:35.72,0:44:36.44,Loithoai,,0000,0000,0000,,Bắn tùy ý.
Dialogue: 0,0:44:56.34,0:44:58.82,Loithoai,,0000,0000,0000,,Nhìn xem.\NNét chữ hoàn toàn khớp.
Dialogue: 0,0:44:59.18,0:45:01.62,Loithoai,,0000,0000,0000,,Đầy những lỗi chính tả.
Dialogue: 0,0:45:04.76,0:45:06.52,Loithoai,,0000,0000,0000,,Nhất định là của Thái Mạo\Nchứ còn ai vào đây nữa.
Dialogue: 0,0:45:06.72,0:45:08.04,Loithoai,,0000,0000,0000,,Phóng tiễn!
Dialogue: 0,0:45:30.59,0:45:32.19,Loithoai,,0000,0000,0000,,Thành công rồi.
Dialogue: 0,0:45:33.47,0:45:34.35,Loithoai,,0000,0000,0000,,Uống đi.
Dialogue: 0,0:45:55.05,0:45:56.69,Loithoai,,0000,0000,0000,,Một lũ chuột nhắt!
Dialogue: 0,0:45:56.88,0:46:00.12,Loithoai,,0000,0000,0000,,Thu binh.
Dialogue: 0,0:46:03.88,0:46:06.32,Loithoai,,0000,0000,0000,,Cảm tạ thừa tướng tặng tên.
Dialogue: 0,0:46:06.55,0:46:08.71,Loithoai,,0000,0000,0000,,Cảm tạ.\NCảm tạ.
Dialogue: 0,0:46:37.01,0:46:37.93,Loithoai,,0000,0000,0000,,Thật tuyệt vời.
Dialogue: 0,0:46:38.18,0:46:38.98,Loithoai,,0000,0000,0000,,Gia Cát tiên sinh.
Dialogue: 0,0:46:40.34,0:46:40.38,Loithoai,,0000,0000,0000,,Ngài nhìn mà xem.
Dialogue: 0,0:46:41.63,0:46:44.55,Loithoai,,0000,0000,0000,,Gia Cát tiên sinh làm sao ngài biết\Nhôm nay sẽ có sương mù lớn vậy?
Dialogue: 0,0:46:44.88,0:46:47.28,Loithoai,,0000,0000,0000,,Liên khuy thiên hà, hữu vân như xà.
Dialogue: 0,0:46:47.51,0:46:48.55,Loithoai,,0000,0000,0000,,chính là lúc có sương lớn.
Dialogue: 0,0:46:48.72,0:46:51.64,Loithoai,,0000,0000,0000,,Nếu như có thể hiểu được thiên văn\Nđịa lý, âm dương thay đổi thế nào
Dialogue: 0,0:46:51.93,0:46:53.85,Loithoai,,0000,0000,0000,,sau đó là nhật, nguyệt, tinh tú
Dialogue: 0,0:46:54.05,0:46:56.17,Loithoai,,0000,0000,0000,,phong lâm sơn hỏa, vạn sự vạn tượng
Dialogue: 0,0:46:56.38,0:46:58.42,Loithoai,,0000,0000,0000,,đều sẽ trở thành binh\Ndưới sự kiểm soát của tôi.
Dialogue: 0,0:46:58.76,0:47:01.48,Loithoai,,0000,0000,0000,,Vậy tính khí của ông trời\Nngài có đọc được không?
Dialogue: 0,0:47:01.84,0:47:03.84,Loithoai,,0000,0000,0000,,Thôi chết. Có một chiếc\Ntrôi đi mất rồi kìa.
Dialogue: 0,0:47:04.34,0:47:06.74,Loithoai,,0000,0000,0000,,Thật ngại quá.\NQuay lại, quay lại.
Dialogue: 0,0:47:49.39,0:47:52.11,Loithoai,,0000,0000,0000,,Chỉ là một trận thắng nhỏ. Đâu cần thừa\Ntướng phải đại giá nghênh đón thế này.
Dialogue: 0,0:47:52.34,0:47:53.22,Loithoai,,0000,0000,0000,,Thừa tướng.
Dialogue: 0,0:47:53.55,0:47:54.83,Loithoai,,0000,0000,0000,,Trận thắng nhỏ?
Dialogue: 0,0:47:55.38,0:47:57.14,Loithoai,,0000,0000,0000,,Quân địch có bao nhiêu tên?
Dialogue: 0,0:47:57.34,0:47:59.42,Loithoai,,0000,0000,0000,,Sương mù lớn che phủ khắp sông.\NKhông thể nào nhìn rõ.
Dialogue: 0,0:47:59.72,0:48:02.08,Loithoai,,0000,0000,0000,,Không nhìn thấy, thế người\Nđánh lui địch thế nào đây?
Dialogue: 0,0:48:02.38,0:48:03.82,Loithoai,,0000,0000,0000,,Dùng loạn tiễn đẩy lui chúng.
Dialogue: 0,0:48:04.01,0:48:05.21,Loithoai,,0000,0000,0000,,Loạn tiễn?
Dialogue: 0,0:48:05.39,0:48:07.23,Loithoai,,0000,0000,0000,,Hết bao nhiêu mũi tên vậy?
Dialogue: 0,0:48:08.47,0:48:11.39,Loithoai,,0000,0000,0000,,Khoảng... 10 vạn tên.
Dialogue: 0,0:48:12.43,0:48:14.03,Loithoai,,0000,0000,0000,,Tại sao không tiếp tục truy kích?
Dialogue: 0,0:48:14.34,0:48:16.02,Loithoai,,0000,0000,0000,,Trên sông sương mù lớn e rằng\Ncó mai phục, thưa thừa tướng.
Dialogue: 0,0:48:16.22,0:48:16.86,Loithoai,,0000,0000,0000,,Vì vậy...
Dialogue: 0,0:48:17.55,0:48:19.15,Loithoai,,0000,0000,0000,,Lại là sương mù lớn?
Dialogue: 0,0:48:19.72,0:48:25.08,Loithoai,,0000,0000,0000,,Xem ra các người có điều giấu lão phu rồi.
Dialogue: 0,0:48:26.97,0:48:29.05,Loithoai,,0000,0000,0000,,Nhìn kìa, trên sông có một\Nthuyền lạ trôi về hướng này.
Dialogue: 0,0:48:29.39,0:48:30.35,Loithoai,,0000,0000,0000,,Kéo vào đây.
Dialogue: 0,0:48:59.22,0:49:00.26,Loithoai,,0000,0000,0000,,Thừa tướng.
Dialogue: 0,0:49:00.97,0:49:04.37,Loithoai,,0000,0000,0000,,Các ngươi, người thật và người rơm\Nmà cũng không phân biệt được sao?
Dialogue: 0,0:49:05.05,0:49:07.65,Loithoai,,0000,0000,0000,,Mười vạn tên?
Dialogue: 0,0:49:08.01,0:49:10.13,Loithoai,,0000,0000,0000,,Các ngươi hào phóng thật đấy.
Dialogue: 0,0:49:10.51,0:49:11.43,Loithoai,,0000,0000,0000,,Thừa tướng xin tha tội.
Dialogue: 0,0:49:11.64,0:49:12.64,Loithoai,,0000,0000,0000,,Mạt tướng tội đáng chết.
Dialogue: 0,0:49:12.93,0:49:14.37,Loithoai,,0000,0000,0000,,Hôm nay mượn tên.
Dialogue: 0,0:49:15.01,0:49:18.29,Loithoai,,0000,0000,0000,,Ngày mai chắc sẽ mượn đầu\Ncủa lão phu chứ gì?
Dialogue: 0,0:49:26.30,0:49:28.22,Loithoai,,0000,0000,0000,,Đây là kế phản gián của Chu Du.
Dialogue: 0,0:49:28.55,0:49:30.99,Loithoai,,0000,0000,0000,,Liệu như chúng tôi tư thông\Nvới địch, đời nào lại dễ dàng...
Dialogue: 0,0:49:31.30,0:49:32.74,Loithoai,,0000,0000,0000,,Im mồm.
Dialogue: 0,0:49:34.22,0:49:37.82,Loithoai,,0000,0000,0000,,Lão phu rất trọng hiền tài,\Ndùng người luôn tin cẩn.
Dialogue: 0,0:49:38.14,0:49:42.74,Loithoai,,0000,0000,0000,,Nhưng lão phu hận nhất là bị lừa,\Nhận nhất là bị phản.
Dialogue: 0,0:49:43.14,0:49:44.50,Loithoai,,0000,0000,0000,,Người đâu?
Dialogue: 0,0:49:45.30,0:49:47.58,Loithoai,,0000,0000,0000,,Thừa tướng, tôi đã ngày đêm\Nhuấn luyện thủy binh...
Dialogue: 0,0:49:47.80,0:49:49.60,Loithoai,,0000,0000,0000,,Dẫn bọn chúng đi.\NTrảm!
Dialogue: 0,0:49:49.80,0:49:52.16,Loithoai,,0000,0000,0000,,Bọn tôi thề chỉ tận trung với thừa tướng
Dialogue: 0,0:49:52.84,0:49:55.08,Loithoai,,0000,0000,0000,,Tại sao ta lại không hàng\Nmột minh chủ cơ chứ?
Dialogue: 0,0:49:55.43,0:49:56.43,Loithoai,,0000,0000,0000,,Bọn ta ngày đêm luyện binh vất vả...
Dialogue: 0,0:49:56.64,0:49:57.68,Loithoai,,0000,0000,0000,,Khoan đã.\NThừa tướng.
Dialogue: 0,0:49:57.89,0:50:00.65,Loithoai,,0000,0000,0000,,Ngài lôi bọn chúng ra chém.\NThủy quân của chúng ta ai sẽ chỉ huy đây.
Dialogue: 0,0:50:01.09,0:50:02.09,Loithoai,,0000,0000,0000,,Dừng lại!
Dialogue: 0,0:50:11.22,0:50:13.10,Loithoai,,0000,0000,0000,,Chín vạn ba ngàn tám trăm
Dialogue: 0,0:50:13.68,0:50:15.52,Loithoai,,0000,0000,0000,,Chín vạn bốn ngàn
Dialogue: 0,0:50:16.05,0:50:18.09,Loithoai,,0000,0000,0000,,Chín vạn bốn ngàn hai trăm
Dialogue: 0,0:50:18.47,0:50:20.27,Loithoai,,0000,0000,0000,,Chín vạn bốn ngàn sáu trăm
Dialogue: 0,0:50:20.80,0:50:23.44,Loithoai,,0000,0000,0000,,Dùng tên của quân Tào bắn quân Tào
Dialogue: 0,0:50:23.76,0:50:26.68,Loithoai,,0000,0000,0000,,Trận này đánh hay lắm đây!
Dialogue: 0,0:50:31.26,0:50:32.54,Loithoai,,0000,0000,0000,,Tập trung vào!\NĐừng có đếm nhầm đấy!
Dialogue: 0,0:50:34.51,0:50:35.63,Loithoai,,0000,0000,0000,,Từ từ.
Dialogue: 0,0:50:35.43,0:50:35.93,Loithoai,,0000,0000,0000,,Ngài đừng có làm ồn nữa được không?
Dialogue: 0,0:50:36.30,0:50:38.46,Loithoai,,0000,0000,0000,,Mỗi người mang hai trăm tên.\NNhầm được chắc?
Dialogue: 0,0:50:39.34,0:50:41.26,Loithoai,,0000,0000,0000,,Gia Cát tiên sinh,\Nphát minh mới này là gì vậy?
Dialogue: 0,0:50:41.47,0:50:42.47,Loithoai,,0000,0000,0000,,Đây là liên nỏ.
Dialogue: 0,0:50:42.64,0:50:44.60,Loithoai,,0000,0000,0000,,Mười mũi tên cùng bắn một lúc,\Nkhông cần nạp lại.
Dialogue: 0,0:50:44.80,0:50:46.08,Loithoai,,0000,0000,0000,,Các ngươi xem.
Dialogue: 0,0:50:46.59,0:50:47.67,Loithoai,,0000,0000,0000,,Tuyệt thật.\NKhông tồi chút nào.
Dialogue: 0,0:50:47.93,0:50:49.53,Loithoai,,0000,0000,0000,,Chín vạn chín ngàn bốn trăm
Dialogue: 0,0:50:49.72,0:50:51.84,Loithoai,,0000,0000,0000,,Chín vạn chín ngàn sáu trăm
Dialogue: 0,0:51:00.39,0:51:01.59,Loithoai,,0000,0000,0000,,Ấn tượng thật.
Dialogue: 0,0:51:01.76,0:51:04.20,Loithoai,,0000,0000,0000,,Không ngờ ngài còn có\Ntài vặt là chế tạo vũ khí nữa
Dialogue: 0,0:51:04.43,0:51:07.71,Loithoai,,0000,0000,0000,,Tài vặt mỗi thứ đều biết một ít,\Ncuộc sống sẽ tốt hơn nhiều đấy.
Dialogue: 0,0:51:08.01,0:51:10.05,Loithoai,,0000,0000,0000,,Đô đốc.
Dialogue: 0,0:51:10.64,0:51:12.32,Loithoai,,0000,0000,0000,,Có đủ 10 vạn không?
Dialogue: 0,0:51:13.26,0:51:16.14,Loithoai,,0000,0000,0000,,Tổng cộng là 99600 mũi tên.
Dialogue: 0,0:51:18.30,0:51:19.98,Loithoai,,0000,0000,0000,,Vậy nên thế nào đây?
Dialogue: 0,0:51:21.01,0:51:22.05,Loithoai,,0000,0000,0000,,Chuyện này...
Dialogue: 0,0:51:25.47,0:51:27.79,Loithoai,,0000,0000,0000,,Chuyện vặt thôi mà,\Nchẳng qua chỉ thiếu 400 mũi tên
Dialogue: 0,0:51:28.01,0:51:30.01,Loithoai,,0000,0000,0000,,Thiếu một mũi cũng là thiếu.
Dialogue: 0,0:51:30.22,0:51:31.66,Loithoai,,0000,0000,0000,,Quân pháp như sơn.
Dialogue: 0,0:51:31.85,0:51:35.65,Loithoai,,0000,0000,0000,,Đúng thế.\NVậy y pháp thì thế nào?
Dialogue: 0,0:51:40.01,0:51:41.65,Loithoai,,0000,0000,0000,,Y pháp xử trảm.
Dialogue: 0,0:51:43.01,0:51:43.97,Loithoai,,0000,0000,0000,,Lũ khốn nạn!
Dialogue: 0,0:51:44.72,0:51:46.04,Loithoai,,0000,0000,0000,,Các ngươi chạy đi đằng nào vậy?
Dialogue: 0,0:51:46.30,0:51:47.42,Loithoai,,0000,0000,0000,,Mau mang qua đây.
Dialogue: 0,0:51:48.01,0:51:48.93,Loithoai,,0000,0000,0000,,Tốt quá rồi.
Dialogue: 0,0:51:49.35,0:51:50.79,Loithoai,,0000,0000,0000,,Tổng cộng là mười vạn hai trăm mũi tên
Dialogue: 0,0:51:50.97,0:51:53.37,Loithoai,,0000,0000,0000,,Gia Cát tiên sinh đã\Nhoàn thành nhiệm vụ rồi.
Dialogue: 0,0:51:53.64,0:51:56.68,Loithoai,,0000,0000,0000,,Xem ra đầu của tôi vẫn còn\Nnguyên vẹn trên cổ rồi.
Dialogue: 0,0:51:57.35,0:51:58.91,Loithoai,,0000,0000,0000,,Còn ngài thì sao?
Dialogue: 0,0:52:02.01,0:52:04.09,Loithoai,,0000,0000,0000,,Ngài muốn đầu của ta sao?
Dialogue: 0,0:52:05.43,0:52:06.55,Loithoai,,0000,0000,0000,,Gia Cát tiên sinh.
Dialogue: 0,0:52:06.72,0:52:07.96,Loithoai,,0000,0000,0000,,Mọi người chỉ đùa thôi mà.
Dialogue: 0,0:52:08.14,0:52:11.42,Loithoai,,0000,0000,0000,,Ban nãy định lấy đầu tôi\Ncũng là anh ta nói đùa chắc?
Dialogue: 0,0:52:12.18,0:52:13.78,Loithoai,,0000,0000,0000,,Đương nhiên không phải nói đùa rồi.
Dialogue: 0,0:52:38.43,0:52:39.55,Loithoai,,0000,0000,0000,,Xem ra đầu của ngài cũng không mất được.
Dialogue: 0,0:52:40.18,0:52:42.54,Loithoai,,0000,0000,0000,,Tào Tháo đã xử trảm\NThái Mạo,Trương Doãn rồi.
Dialogue: 0,0:52:51.35,0:52:54.11,Loithoai,,0000,0000,0000,,Hai chúng tôi mới là người bị lừa thì có.
Dialogue: 0,0:52:54.68,0:52:57.72,Loithoai,,0000,0000,0000,,Háo địch chi tư.\NLoạn địch chi tâm.
Dialogue: 0,0:52:57.97,0:53:01.25,Loithoai,,0000,0000,0000,,Liên hoàn kế của hai người\Nquả là cao minh.
Dialogue: 0,0:53:03.43,0:53:05.23,Loithoai,,0000,0000,0000,,Tôi nghĩ chắc hẳn giờ này\NTào Tháo đang tức tối lắm đây.
Dialogue: 0,0:53:05.64,0:53:08.64,Loithoai,,0000,0000,0000,,Nội trong 2, 3 ngày tới hắn nhất định\Nsẽ liều lĩnh hành động.
Dialogue: 0,0:53:08.89,0:53:11.41,Loithoai,,0000,0000,0000,,Tôi thấy phen này Tưởng Can nguy to rồi.
Dialogue: 0,0:53:14.35,0:53:16.35,Loithoai,,0000,0000,0000,,Thật đáng tiếc cho người bạn này.
Dialogue: 0,0:53:19.39,0:53:20.55,Loithoai,,0000,0000,0000,,Chư vị.
Dialogue: 0,0:53:20.85,0:53:22.97,Loithoai,,0000,0000,0000,,Gã Chu Du đó chỉ mới phô trương\Nđược chút tài cỏn con.
Dialogue: 0,0:53:23.18,0:53:25.70,Loithoai,,0000,0000,0000,,Nhất định hắn tưởng lão phu đang rất tức tối.
Dialogue: 0,0:53:25.93,0:53:27.57,Loithoai,,0000,0000,0000,,Các người nhìn xem ta có vẻ gì\Nlà đang tức tối không?
Dialogue: 0,0:53:27.76,0:53:29.28,Loithoai,,0000,0000,0000,,Không hề, không hề!
Dialogue: 0,0:53:30.76,0:53:32.52,Loithoai,,0000,0000,0000,,Nào, giờ đã vạch mặt được tên nội gián.
Dialogue: 0,0:53:32.72,0:53:35.28,Loithoai,,0000,0000,0000,,Sau này không cần phải lo âu nữa rồi.\NCạn ly!
Dialogue: 0,0:53:37.14,0:53:38.18,Loithoai,,0000,0000,0000,,Tương Can.
Dialogue: 0,0:53:38.56,0:53:38.96,Loithoai,,0000,0000,0000,,Có!
Dialogue: 0,0:53:39.47,0:53:41.91,Loithoai,,0000,0000,0000,,Lần này công trạng của ngươi là lớn nhất.
Dialogue: 0,0:53:42.72,0:53:43.92,Loithoai,,0000,0000,0000,,Uống một ly đi.
Dialogue: 0,0:53:50.97,0:53:52.17,Loithoai,,0000,0000,0000,,Tạ thừa tướng!
Dialogue: 0,0:54:03.97,0:54:04.77,Loithoai,,0000,0000,0000,,Đại nhân.
Dialogue: 0,0:54:10.85,0:54:12.05,Loithoai,,0000,0000,0000,,Đại nhân! Ngài...
Dialogue: 0,0:54:17.01,0:54:19.13,Loithoai,,0000,0000,0000,,Mình ngươi ngu dốt là đủ rồi,
Dialogue: 0,0:54:19.76,0:54:22.04,Loithoai,,0000,0000,0000,,nhưng không thể để sự ngu dốt\Ncủa ngươi ảnh hưởng tới ta được.
Dialogue: 0,0:54:26.93,0:54:28.17,Loithoai,,0000,0000,0000,,Truyền lệnh.
Dialogue: 0,0:54:29.85,0:54:32.45,Loithoai,,0000,0000,0000,,Nội trong 2 ngày tới, đại quân vượt sông,
Dialogue: 0,0:54:32.81,0:54:33.85,Loithoai,,0000,0000,0000,,đoạt lấy Xích Bích.
Dialogue: 0,0:54:34.06,0:54:35.14,Loithoai,,0000,0000,0000,,Tuân lệnh!
Dialogue: 0,0:54:42.76,0:54:47.08,Loithoai,,0000,0000,0000,,Thừa tướng có lệnh.\NNội trong 2 ngày, đại quân qua sông,
Dialogue: 0,0:55:03.72,0:55:04.84,Loithoai,,0000,0000,0000,,Làm trò gì vậy?
Dialogue: 0,0:55:07.60,0:55:09.48,Loithoai,,0000,0000,0000,,Có gian tế!\NBắt lấy gian tế!
Dialogue: 0,0:55:16.85,0:55:17.85,Loithoai,,0000,0000,0000,,Đứng lại!
Dialogue: 0,0:55:24.81,0:55:26.13,Loithoai,,0000,0000,0000,,Đừng cho hắn chạy thoát!
Dialogue: 0,0:55:27.89,0:55:28.73,Loithoai,,0000,0000,0000,,Mau tóm lấy hắn!
Dialogue: 0,0:55:49.97,0:55:51.93,Loithoai,,0000,0000,0000,,Bàn Trư, sao bọn họ lại đánh cậu?
Dialogue: 0,0:55:52.18,0:55:52.98,Loithoai,,0000,0000,0000,,Có hiểu lầm!
Dialogue: 0,0:55:53.35,0:55:54.31,Loithoai,,0000,0000,0000,,Hiểu lầm à?
Dialogue: 0,0:55:55.18,0:55:56.30,Loithoai,,0000,0000,0000,,Mấy người kia đang làm trò gì vậy?
Dialogue: 0,0:55:56.43,0:55:57.47,Loithoai,,0000,0000,0000,,Gian tế!\NHắn là gian tế!
Dialogue: 0,0:55:57.64,0:55:59.04,Loithoai,,0000,0000,0000,,Nói bậy, cậu ta là bạn tốt của ta.
Dialogue: 0,0:55:59.22,0:56:01.14,Loithoai,,0000,0000,0000,,Bạn tốt cái con khỉ?\NNgươi bị ấm đầu à?
Dialogue: 0,0:56:02.64,0:56:04.60,Loithoai,,0000,0000,0000,,Đi thôi, chúng nó đánh thiên phu trưởng.
Dialogue: 0,0:56:04.85,0:56:06.17,Loithoai,,0000,0000,0000,,Có đánh nhau rồi!
Dialogue: 0,0:56:14.14,0:56:14.82,Loithoai,,0000,0000,0000,,Sao lại đánh nhau nhỉ?
Dialogue: 0,0:56:14.97,0:56:16.53,Loithoai,,0000,0000,0000,,Có gì đâu, chắc là giỡn chơi ấy mà.
Dialogue: 0,0:56:33.13,0:56:34.40,Loithoai,,0000,0000,0000,,Thúc Tài.
Dialogue: 0,0:56:36.21,0:56:36.86,Loithoai,,0000,0000,0000,,Thúc Tài.
Dialogue: 0,0:56:41.81,0:56:43.49,Loithoai,,0000,0000,0000,,Bàn Trư, cậu không sao chứ?
Dialogue: 0,0:56:49.18,0:56:50.34,Loithoai,,0000,0000,0000,,Tôi phải đi rồi.
Dialogue: 0,0:56:56.85,0:56:58.01,Loithoai,,0000,0000,0000,,Bàn Trư.
Dialogue: 0,0:56:58.89,0:57:00.53,Loithoai,,0000,0000,0000,,Cậu định đi đâu?
Dialogue: 0,0:57:00.76,0:57:03.36,Loithoai,,0000,0000,0000,,Mẹ tôi bị bệnh.\NGiờ tôi phải về nhà.
Dialogue: 0,0:57:03.81,0:57:06.61,Loithoai,,0000,0000,0000,,Cậu thật hiếu thảo,\Nđúng là nên về nhà thăm bà ấy.
Dialogue: 0,0:57:12.22,0:57:13.18,Loithoai,,0000,0000,0000,,Cậu cũng nên về nhà đi.
Dialogue: 0,0:57:13.35,0:57:15.43,Loithoai,,0000,0000,0000,,Không cần, đánh trận\Nkhải hoàn tôi sẽ trở về.
Dialogue: 0,0:57:15.68,0:57:16.80,Loithoai,,0000,0000,0000,,Đánh trận xong không cần phải đóng thuế.
Dialogue: 0,0:57:16.97,0:57:18.49,Loithoai,,0000,0000,0000,,Trong nhà có thể có nhiều cơm ăn hơn.
Dialogue: 0,0:57:18.97,0:57:20.49,Loithoai,,0000,0000,0000,,Cậu mau về thăm mẹ mình đi.
Dialogue: 0,0:57:27.93,0:57:29.65,Loithoai,,0000,0000,0000,,Tôi sẽ quay lại gặp cậu.
Dialogue: 0,0:57:37.64,0:57:38.72,Loithoai,,0000,0000,0000,,Công kênh nào.
Dialogue: 0,0:57:51.77,0:57:53.17,Loithoai,,0000,0000,0000,,Đừng để hắn trốn mất!
Dialogue: 0,0:57:53.60,0:57:55.36,Loithoai,,0000,0000,0000,,Cậu ta chỉ về thăm mẹ thôi.
Dialogue: 0,0:58:08.39,0:58:10.35,Loithoai,,0000,0000,0000,,Đánh nữa đi!\NĐấm mạnh vào!
Dialogue: 0,0:58:13.64,0:58:14.84,Loithoai,,0000,0000,0000,,Khởi binh 18 năm nay,
Dialogue: 0,0:58:15.02,0:58:17.30,Loithoai,,0000,0000,0000,,chinh chiến hơn trăm trận lớn nhỏ\Ncó lần nào ta chưa từng chiến thắng.
Dialogue: 0,0:58:17.52,0:58:20.36,Loithoai,,0000,0000,0000,,Nếu như ngay cả một Xích Bích\Ncỏn con này mà còn không hạ được,
Dialogue: 0,0:58:20.60,0:58:22.56,Loithoai,,0000,0000,0000,,thật không hiểu nổi sau này\Nsử sách biết viết thế nào.
Dialogue: 0,0:58:23.18,0:58:23.54,Loithoai,,0000,0000,0000,,Đúng vậy.
Dialogue: 0,0:58:30.02,0:58:31.74,Loithoai,,0000,0000,0000,,Thừa tướng.
Dialogue: 0,0:58:45.47,0:58:46.27,Loithoai,,0000,0000,0000,,Sao rồi?
Dialogue: 0,0:58:47.64,0:58:49.28,Loithoai,,0000,0000,0000,,Tình hình đúng là tồi tệ hơn dự tính.
Dialogue: 0,0:58:49.97,0:58:52.89,Loithoai,,0000,0000,0000,,Bệnh dịch không ngừng lan nhanh.
Dialogue: 0,0:58:54.93,0:58:55.97,Loithoai,,0000,0000,0000,,Với tình hình này,
Dialogue: 0,0:58:56.14,0:58:58.50,Loithoai,,0000,0000,0000,,nhanh nhất cũng phải hơn 20 ngày\Nmới có thể kiểm soát được.
Dialogue: 0,0:59:01.85,0:59:05.25,Loithoai,,0000,0000,0000,,Hoa Đà mạo muội thỉnh cầu\Ntạm ngừng xuất binh.
Dialogue: 0,0:59:17.23,0:59:18.03,Loithoai,,0000,0000,0000,,Đừng cử động!
Dialogue: 0,0:59:18.18,0:59:18.94,Loithoai,,0000,0000,0000,,Cứ nằm đó!
Dialogue: 0,0:59:19.10,0:59:19.74,Loithoai,,0000,0000,0000,,Thừa tướng.
Dialogue: 0,0:59:19.93,0:59:21.13,Loithoai,,0000,0000,0000,,Đứng lên đi.
Dialogue: 0,0:59:21.60,0:59:24.20,Loithoai,,0000,0000,0000,,Các ngươi đang bệnh cần phải dưỡng sức.
Dialogue: 0,0:59:27.31,0:59:30.19,Loithoai,,0000,0000,0000,,Man Đồn, sao ngươi cũng bệnh vậy?
Dialogue: 0,0:59:31.52,0:59:33.32,Loithoai,,0000,0000,0000,,Bình thường không phải\Nsức khỏe tốt lắm sao?
Dialogue: 0,0:59:34.81,0:59:36.13,Loithoai,,0000,0000,0000,,Muốn gì nào?
Dialogue: 0,0:59:37.39,0:59:38.35,Loithoai,,0000,0000,0000,,Nói đi!
Dialogue: 0,0:59:39.31,0:59:41.51,Loithoai,,0000,0000,0000,,Tôi muốn về nhà.
Dialogue: 0,0:59:43.98,0:59:46.34,Loithoai,,0000,0000,0000,,Ta cũng đã lâu lắm rồi chưa về nhà,
Dialogue: 0,0:59:47.14,0:59:49.62,Loithoai,,0000,0000,0000,,cũng muốn quay về thăm\Nđứa con trai nhỏ của ta.
Dialogue: 0,0:59:54.02,0:59:55.94,Loithoai,,0000,0000,0000,,Nó tên là Tào Xung.
Dialogue: 0,0:59:57.93,1:00:00.85,Loithoai,,0000,0000,0000,,Năm nay 13 tuổi, còn nhỏ hơn cả ngươi.
Dialogue: 0,1:00:01.68,1:00:04.76,Loithoai,,0000,0000,0000,,Nó rất hiền lành, cũng rất thông minh nữa.
Dialogue: 0,1:00:05.35,1:00:09.87,Loithoai,,0000,0000,0000,,Từ nhỏ sức khỏe của nó đã không\Nđược tốt, thường hay đổ bệnh.
Dialogue: 0,1:00:11.10,1:00:13.10,Loithoai,,0000,0000,0000,,Thế nhưng trước mặt ta,
Dialogue: 0,1:00:13.60,1:00:16.60,Loithoai,,0000,0000,0000,,nó luôn tỏ vẻ chẳng có chuyện gì cả.
Dialogue: 0,1:00:18.89,1:00:22.01,Loithoai,,0000,0000,0000,,Dù có bệnh gì đi nữa,\Nnó luôn tự mình chịu đựng
Dialogue: 0,1:00:22.56,1:00:24.68,Loithoai,,0000,0000,0000,,chẳng bao giờ than vãn với ta.
Dialogue: 0,1:00:46.31,1:00:50.67,Loithoai,,0000,0000,0000,,Ngay từ lúc đầu khởi binh ta thề rằng
Dialogue: 0,1:00:51.02,1:00:55.30,Loithoai,,0000,0000,0000,,nhất định phải quét sạch quần hùng.
Dialogue: 0,1:01:00.89,1:01:05.01,Loithoai,,0000,0000,0000,,Chúng ta nhất định phải đoạt lấy\NXích Bích, công hạ sài tang.
Dialogue: 0,1:01:07.43,1:01:12.35,Loithoai,,0000,0000,0000,,Sau đó hãy mang một vài\Nnắm đất ở đây trở về quê hương.
Dialogue: 0,1:01:13.48,1:01:18.32,Loithoai,,0000,0000,0000,,Và hãy báo với người trong nhà\Nchúng ta đã ca khúc khải hoàn.
Dialogue: 0,1:01:19.64,1:01:22.04,Loithoai,,0000,0000,0000,,Chúng ta thắng rồi.
Dialogue: 0,1:01:23.93,1:01:28.05,Loithoai,,0000,0000,0000,,Vì vậy các người nhất định phải mạnh mẽ lên.
Dialogue: 0,1:01:30.77,1:01:34.33,Loithoai,,0000,0000,0000,,Đương nhiên ta cũng cần phải vậy.
Dialogue: 0,1:01:35.56,1:01:41.84,Loithoai,,0000,0000,0000,,Bởi vì ta cũng cần đưa mọi người trở về.
Dialogue: 0,1:02:25.06,1:03:17.98,Loithoai,,0000,0000,0000,,THẮNG LỢI! THẮNG LỢI!
Dialogue: 0,1:03:42.60,1:03:44.08,Loithoai,,0000,0000,0000,,Chào mọi người.
Dialogue: 0,1:03:44.56,1:03:45.28,Loithoai,,0000,0000,0000,,Đã để mọi người phải chờ lâu rồi.
Dialogue: 0,1:03:45.48,1:03:46.24,Loithoai,,0000,0000,0000,,Trở về đúng lúc lắm.
Dialogue: 0,1:03:47.89,1:03:49.33,Loithoai,,0000,0000,0000,,Không có bất trắc gì chứ?
Dialogue: 0,1:03:50.10,1:03:50.14,Loithoai,,0000,0000,0000,,Không.
Dialogue: 0,1:03:50.52,1:03:53.48,Loithoai,,0000,0000,0000,,Chỉ là ngã ngay xuống bùn khi\Nnhảy qua hàng rào doanh trại.
Dialogue: 0,1:03:54.48,1:03:56.00,Loithoai,,0000,0000,0000,,Thượng Hương.
Dialogue: 0,1:03:57.39,1:03:58.47,Loithoai,,0000,0000,0000,,Muội suốt ngày chỉ toàn gây rắc rồi thôi.
Dialogue: 0,1:03:58.64,1:04:00.92,Loithoai,,0000,0000,0000,,Mấy ngày qua muội biến đi đâu vậy?
Dialogue: 0,1:04:05.19,1:04:07.31,Loithoai,,0000,0000,0000,,Bọn chúng chuẩn bị khai chiến rồi.
Dialogue: 0,1:04:08.02,1:04:09.26,Loithoai,,0000,0000,0000,,Thượng Hương.
Dialogue: 0,1:04:10.27,1:04:11.51,Loithoai,,0000,0000,0000,,Muội đã trở về.
Dialogue: 0,1:04:11.73,1:04:12.53,Loithoai,,0000,0000,0000,,Tiểu Kiều
Dialogue: 0,1:04:17.19,1:04:18.67,Loithoai,,0000,0000,0000,,Thượng Hương?
Dialogue: 0,1:04:31.19,1:04:32.55,Loithoai,,0000,0000,0000,,Tiểu Kiều, nào.
Dialogue: 0,1:04:32.85,1:04:34.21,Loithoai,,0000,0000,0000,,Kéo ra giúp muội.
Dialogue: 0,1:04:34.44,1:04:35.88,Loithoai,,0000,0000,0000,,Nào, kéo đi.
Dialogue: 0,1:04:35.94,1:04:36.14,Loithoai,,0000,0000,0000,,Nào, kéo đi.
Dialogue: 0,1:05:05.85,1:05:08.01,Loithoai,,0000,0000,0000,,Đây chính là bản đồ doanh trại quân Tào.
Dialogue: 0,1:05:08.81,1:05:09.97,Loithoai,,0000,0000,0000,,Trong đây là kỳ vân các.
Dialogue: 0,1:05:10.35,1:05:11.91,Loithoai,,0000,0000,0000,,Là đại trướng nơi Tào Tháo ngồi bàn việc.
Dialogue: 0,1:05:12.10,1:05:14.14,Loithoai,,0000,0000,0000,,Nơi đây có Thanh Châu,\Nbinh đoàn tinh nhuệ nhất hộ vệ.
Dialogue: 0,1:05:14.35,1:05:15.95,Loithoai,,0000,0000,0000,,Doanh trại binh lính nằm trong đây.
Dialogue: 0,1:05:16.15,1:05:17.79,Loithoai,,0000,0000,0000,,Đông Tây hai phía đều có trọng binh.
Dialogue: 0,1:05:17.98,1:05:19.58,Loithoai,,0000,0000,0000,,Phía sau núi binh lực mỏng nhất.
Dialogue: 0,1:05:19.77,1:05:21.77,Loithoai,,0000,0000,0000,,giống như hổ già chỉ lo\Nđề phòng phía trước,
Dialogue: 0,1:05:21.98,1:05:23.26,Loithoai,,0000,0000,0000,,chứ không thèm cảnh giác phía sau.
Dialogue: 0,1:05:23.48,1:05:24.44,Loithoai,,0000,0000,0000,,- Thủy trại của quân Tào.\N- Mặc vào nào.
Dialogue: 0,1:05:24.60,1:05:25.72,Loithoai,,0000,0000,0000,,- Coi chừng bị cảm lạnh đấy.\N- Muội không lạnh.
Dialogue: 0,1:05:25.90,1:05:27.02,Loithoai,,0000,0000,0000,,3 phần binh lực của chúng
Dialogue: 0,1:05:27.19,1:05:29.19,Loithoai,,0000,0000,0000,,đang ở trên thuyền sẵn sàng chiến đầu.
Dialogue: 0,1:05:29.39,1:05:31.55,Loithoai,,0000,0000,0000,,Chiến thuyền ở đằng trước,\Nthuyền rút lui ở đằng sau.
Dialogue: 0,1:05:31.77,1:05:33.45,Loithoai,,0000,0000,0000,,thuyền nhỏ ở bên trong,\Ncó thể tự do di chuyển.
Dialogue: 0,1:05:33.65,1:05:35.33,Loithoai,,0000,0000,0000,,Đây là "thủy thượng liên hoàn trận".
Dialogue: 0,1:05:35.73,1:05:36.13,Loithoai,,0000,0000,0000,,Không cần.
Dialogue: 0,1:05:36.27,1:05:38.35,Loithoai,,0000,0000,0000,,Có thể hỗ trợ lẫn nhau, khả công khả thủ.
Dialogue: 0,1:05:58.10,1:06:00.46,Loithoai,,0000,0000,0000,,Ta trách lầm muội rồi.
Dialogue: 0,1:06:08.06,1:06:09.82,Loithoai,,0000,0000,0000,,Nếu như ngươi là Chu Du,
Dialogue: 0,1:06:10.02,1:06:12.46,Loithoai,,0000,0000,0000,,ngươi sẽ dùng cách gì để đánh chúng ta?
Dialogue: 0,1:06:13.35,1:06:15.39,Loithoai,,0000,0000,0000,,Binh lực của hắn ít chỉ đủ đánh một trận,
Dialogue: 0,1:06:15.60,1:06:17.60,Loithoai,,0000,0000,0000,,chỉ còn cách dốc toàn lực công kích\Nđội thuyền của chúng ta.
Dialogue: 0,1:06:18.73,1:06:21.37,Loithoai,,0000,0000,0000,,Ta đoán cách tối ưu nhất của\Nbọn chúng là dùng hỏa công.
Dialogue: 0,1:06:21.77,1:06:23.57,Loithoai,,0000,0000,0000,,Hỏa công? Không thể nào.
Dialogue: 0,1:06:23.77,1:06:26.41,Loithoai,,0000,0000,0000,,Binh lực hiện tại của chúng không đủ,\Ntất nhiên cần mượn sức mạnh.
Dialogue: 0,1:06:26.69,1:06:29.29,Loithoai,,0000,0000,0000,,Cách nào dễ dàng nhất để làm được điều đó đây?\NDĩ nhiên là dùng lửa rồi.
Dialogue: 0,1:06:29.52,1:06:31.24,Loithoai,,0000,0000,0000,,Trong binh pháp có đề cập tới điều này.
Dialogue: 0,1:06:31.65,1:06:33.61,Loithoai,,0000,0000,0000,,Đáng tiếc ý đồ của chúng\Nkhông thực hiện được rồi.
Dialogue: 0,1:06:34.40,1:06:36.48,Loithoai,,0000,0000,0000,,Hiện giờ gió Tây Bắc đang thổi.
Dialogue: 0,1:06:37.73,1:06:40.33,Loithoai,,0000,0000,0000,,Nếu như bọn chúng định dùng lửa\Nđể hỏa thiêu thuyền đội của chúng ta,
Dialogue: 0,1:06:41.10,1:06:44.98,Loithoai,,0000,0000,0000,,gió sẽ thổi tạt lửa lại, tất nhiên\Nsẽ thiêu cháy chính bọn chúng.
Dialogue: 0,1:06:46.19,1:06:48.71,Loithoai,,0000,0000,0000,,Chúng ta mới chính là những người\Nnên dùng hỏa công mới đúng.
Dialogue: 0,1:06:49.52,1:06:52.96,Loithoai,,0000,0000,0000,,Phải rồi. Lệnh cho tất cả mang thật nhiều\Nlưu huỳnh lên thuyền, tối mai phát binh.
Dialogue: 0,1:06:53.27,1:06:56.67,Loithoai,,0000,0000,0000,,Ta sẽ cho chúng biết\Nthế nào là hỏa thiêu Xích Bích.
Dialogue: 0,1:07:04.48,1:07:06.04,Loithoai,,0000,0000,0000,,Từ trận hình đồ này có thể thấy
Dialogue: 0,1:07:06.44,1:07:08.64,Loithoai,,0000,0000,0000,,Tào Tháo đã triển khai xong xuôi\Nđội hình khai chiến rồi.
Dialogue: 0,1:07:08.85,1:07:12.53,Loithoai,,0000,0000,0000,,Rất có thể tối nay sẽ xảy ra trận quyết chiến.
Dialogue: 0,1:07:14.02,1:07:16.06,Loithoai,,0000,0000,0000,,Đây là trận chiến sinh tử.
Dialogue: 0,1:07:16.94,1:07:21.26,Loithoai,,0000,0000,0000,,Cho dù thắng bại ra sao, chúng ta\Ncũng phải dốc toàn lực chiến đấu.
Dialogue: 0,1:07:35.02,1:07:36.14,Loithoai,,0000,0000,0000,,Thượng Hương
Dialogue: 0,1:07:36.52,1:07:38.52,Loithoai,,0000,0000,0000,,Đường nối giữa những chiếc thuyền này là ý gì?
Dialogue: 0,1:07:38.77,1:07:39.53,Loithoai,,0000,0000,0000,,Nói ra thì buồn cười lắm.
Dialogue: 0,1:07:39.81,1:07:42.85,Loithoai,,0000,0000,0000,,Thuyền chiến của bọn chúng đều\Nđược dùng xích sắt kết nối với nhau,
Dialogue: 0,1:07:43.19,1:07:44.47,Loithoai,,0000,0000,0000,,để cố định cả đội thuyền.
Dialogue: 0,1:07:44.65,1:07:47.77,Loithoai,,0000,0000,0000,,Làm thế này người phương Bắc\Nsẽ không còn bị say sóng nữa.
Dialogue: 0,1:07:48.06,1:07:48.98,Loithoai,,0000,0000,0000,,Nói chuyện cả nửa ngày rồi.
Dialogue: 0,1:07:49.15,1:07:50.51,Loithoai,,0000,0000,0000,,Rốt cuộc sách lược của ngài là gì?
Dialogue: 0,1:07:59.06,1:08:01.06,Loithoai,,0000,0000,0000,,Tào Tháo dựng trại ở nơi địa thế bất lợi.
Dialogue: 0,1:08:01.94,1:08:03.18,Loithoai,,0000,0000,0000,,Bên đó là quải hình
Dialogue: 0,1:08:03.35,1:08:05.19,Loithoai,,0000,0000,0000,,lui lại rất khó khăn.
Dialogue: 0,1:08:05.44,1:08:08.48,Loithoai,,0000,0000,0000,,Nếu như vượt sông công kích không thành công,\Nbọn chúng không thể quay lại ngay lập tức được.
Dialogue: 0,1:08:09.77,1:08:11.45,Loithoai,,0000,0000,0000,,Chúng ta có thể dùng hỏa công.
Dialogue: 0,1:08:11.81,1:08:13.73,Loithoai,,0000,0000,0000,,Địa hình doanh trại quân Tào\Nrất có lợi với chúng ta.
Dialogue: 0,1:08:14.06,1:08:16.94,Loithoai,,0000,0000,0000,,Chỉ cần chúng ta từ chính diện hỏa thiêu\Nthuyền trận của quân Tào,
Dialogue: 0,1:08:17.15,1:08:19.59,Loithoai,,0000,0000,0000,,ngay lập tức bọn chúng tiến lui\Nchẳng được, vô lực hồi thiên rồi.
Dialogue: 0,1:08:19.94,1:08:21.82,Loithoai,,0000,0000,0000,,Đúng rồi, dùng hỏa công.
Dialogue: 0,1:08:22.56,1:08:25.96,Loithoai,,0000,0000,0000,,Tôi chỉ cần 10 hỏa thuyền,
Dialogue: 0,1:08:26.23,1:08:28.83,Loithoai,,0000,0000,0000,,hai chiếc sẽ xông vào cánh tả hữu
Dialogue: 0,1:08:29.19,1:08:33.51,Loithoai,,0000,0000,0000,,Tám chiếc thuyền chủ lực sẽ đâm vào\Nchính giữa thủy doanh của Tào quân
Dialogue: 0,1:08:33.90,1:08:35.54,Loithoai,,0000,0000,0000,,Từ trong đây châm lửa,
Dialogue: 0,1:08:35.77,1:08:38.21,Loithoai,,0000,0000,0000,,ngay sau đó các vị có thể\Ntheo đó xông vào rồi
Dialogue: 0,1:08:38.44,1:08:41.28,Loithoai,,0000,0000,0000,,Dù cho binh lực của chúng có nhiều tới đâu
Dialogue: 0,1:08:41.56,1:08:43.64,Loithoai,,0000,0000,0000,,chúng ta cũng sẽ quét sạch bọn chúng.
Dialogue: 0,1:08:49.65,1:08:51.53,Loithoai,,0000,0000,0000,,Nếu như đội kỵ binh của\NLưu Dự Châu vẫn còn ở đây,
Dialogue: 0,1:08:51.73,1:08:54.57,Loithoai,,0000,0000,0000,,ngay sau đó từ phía sau Tào doanh\Nđã có thể khởi hành công kích.
Dialogue: 0,1:08:54.81,1:08:57.33,Loithoai,,0000,0000,0000,,Nếu thế này chúng ta có thể\Nđại địch toàn thắng rồi.
Dialogue: 0,1:09:11.44,1:09:13.92,Loithoai,,0000,0000,0000,,Hướng gió này có lợi với quân Tào.
Dialogue: 0,1:09:15.98,1:09:17.70,Loithoai,,0000,0000,0000,,Hiện giờ gió Tây Bắc đang thổi.
Dialogue: 0,1:09:17.90,1:09:20.06,Loithoai,,0000,0000,0000,,Dúng hỏa thuyền công kích sẽ bị gió\Ntạt lại chính chúng ta sẽ bị hỏa thiêu.
Dialogue: 0,1:09:20.27,1:09:22.39,Loithoai,,0000,0000,0000,,Tào Tháo cũng biết lợi dụng hướng gió,
Dialogue: 0,1:09:22.81,1:09:24.93,Loithoai,,0000,0000,0000,,chắc hẳn cũng nghĩ tới chuyện dùng hỏa công.
Dialogue: 0,1:09:53.40,1:09:54.64,Loithoai,,0000,0000,0000,,Sao ngài không nói gì cả vậy?
Dialogue: 0,1:09:55.06,1:09:56.54,Loithoai,,0000,0000,0000,,Đang quan sát gì thế?
Dialogue: 0,1:09:57.69,1:10:00.05,Loithoai,,0000,0000,0000,,Người nhìn xem, những đám\Nmây đen ở phía Đông kia
Dialogue: 0,1:10:00.27,1:10:03.03,Loithoai,,0000,0000,0000,,kéo tới che phủ cả mặt trời,\Nhỏa khí ngút trời.
Dialogue: 0,1:10:03.31,1:10:06.03,Loithoai,,0000,0000,0000,,Chẳng phải là bất thường lắm sao?
Dialogue: 0,1:10:06.31,1:10:08.51,Loithoai,,0000,0000,0000,,Khổng Minh, ý của ngài là gì?
Dialogue: 0,1:10:09.02,1:10:10.66,Loithoai,,0000,0000,0000,,Gió sẽ đổi chiều.
Dialogue: 0,1:10:14.27,1:10:15.75,Loithoai,,0000,0000,0000,,Qua nhiều năm làm nghề nông,\Ntôi đã rút ra kinh nghiệm,
Dialogue: 0,1:10:15.97,1:10:19.53,Loithoai,,0000,0000,0000,,nếu như mùa đông mây che phủ\Nmặt trời, hỏa khí ngút trời
Dialogue: 0,1:10:20.14,1:10:22.49,Loithoai,,0000,0000,0000,,thi khi đó 9 ngọn núi dọc\Ntheo sông Trường Giang
Dialogue: 0,1:10:22.87,1:10:23.73,Loithoai,,0000,0000,0000,,sẽ xuất hiện lục hồ phong.
Dialogue: 0,1:10:24.06,1:10:25.71,Loithoai,,0000,0000,0000,,Thế nào là lục hồ phong?
Dialogue: 0,1:10:26.07,1:10:27.61,Loithoai,,0000,0000,0000,,Chính là gió ấm Đông Nam.
Dialogue: 0,1:10:28.02,1:10:31.00,Loithoai,,0000,0000,0000,,Ngọn gió này thổi từ\NĐông Nam sang Tây Bắc.
Dialogue: 0,1:10:32.24,1:10:34.75,Loithoai,,0000,0000,0000,,Liệu Tào Tháo có biết\Nđược điều này không?
Dialogue: 0,1:10:35.21,1:10:38.51,Loithoai,,0000,0000,0000,,Chỉ duy nhất bên đó có Thái Mạo biết,\Nđiều này nhưng hắn đã bị xử tử mất rồi.
Dialogue: 0,1:10:44.56,1:10:45.50,Loithoai,,0000,0000,0000,,Khổng Minh.
Dialogue: 0,1:10:45.95,1:10:47.41,Loithoai,,0000,0000,0000,,Ngài có thể đoán trước được\Nkhi nào gió sẽ đổi chiều không?
Dialogue: 0,1:10:47.75,1:10:48.75,Loithoai,,0000,0000,0000,,Chính là đêm nay hoặc sớm mai.
Dialogue: 0,1:10:50.36,1:10:53.07,Loithoai,,0000,0000,0000,,Từ xưa tới nay thời tiết là yếu tố quan trọng.
Dialogue: 0,1:10:53.53,1:10:55.07,Loithoai,,0000,0000,0000,,Người nào có thể nắm bắt được\Nđiều này, người đó sẽ thắng.
Dialogue: 0,1:11:01.73,1:11:02.98,Loithoai,,0000,0000,0000,,Chúng ta phải tìm ra biện pháp\Nngăn chặn Tào Tháo khởi binh.
Dialogue: 0,1:11:05.51,1:11:07.54,Loithoai,,0000,0000,0000,,Chỉ như vậy mới có cơ hội thắng địch.
Dialogue: 0,1:11:31.91,1:11:33.13,Loithoai,,0000,0000,0000,,Chào tướng quân.
Dialogue: 0,1:11:40.03,1:11:40.91,Loithoai,,0000,0000,0000,,Quả là không tồi.
Dialogue: 0,1:11:41.38,1:11:42.55,Loithoai,,0000,0000,0000,,Thứ dầu cá này là công hiệu nhất.
Dialogue: 0,1:12:12.09,1:12:12.78,Loithoai,,0000,0000,0000,,To hơn chút nữa.
Dialogue: 0,1:12:13.10,1:12:13.77,Loithoai,,0000,0000,0000,,Rõ.
Dialogue: 0,1:12:25.98,1:12:27.22,Loithoai,,0000,0000,0000,,Không được.\NTo hơn nữa.
Dialogue: 0,1:12:27.58,1:12:28.39,Loithoai,,0000,0000,0000,,Rõ.
Dialogue: 0,1:12:38.71,1:12:39.91,Loithoai,,0000,0000,0000,,Không được.\NTo hơn nữa đi.
Dialogue: 0,1:12:41.47,1:12:42.45,Loithoai,,0000,0000,0000,,Vậy ngài muốn to bằng cỡ nào?
Dialogue: 0,1:12:43.78,1:12:44.49,Loithoai,,0000,0000,0000,,Đi đi.
Dialogue: 0,1:13:01.53,1:13:03.04,Loithoai,,0000,0000,0000,,Hình như có hơi nhiều.
Dialogue: 0,1:13:06.12,1:13:07.42,Loithoai,,0000,0000,0000,,Thôi được rồi.\NCái này được.
Dialogue: 0,1:13:18.65,1:13:21.53,Loithoai,,0000,0000,0000,,Ngài xem, gió Tây Bắc vẫn\Ncòn thổi mạnh thế này.
Dialogue: 0,1:13:21.85,1:13:23.17,Loithoai,,0000,0000,0000,,Gió liệu có thể đổi chiều được không đây?
Dialogue: 0,1:13:23.32,1:13:26.12,Loithoai,,0000,0000,0000,,Chúng ta vốn dĩ chẳng thể có cách\Nnào tiếp cận thủy trại của chúng được.
Dialogue: 0,1:13:27.47,1:13:29.75,Loithoai,,0000,0000,0000,,Đô đốc, hãy dùng "khổ nhục kế"
Dialogue: 0,1:13:30.39,1:13:31.78,Loithoai,,0000,0000,0000,,ngài cứ tìm lý do nào đó
Dialogue: 0,1:13:32.21,1:13:33.71,Loithoai,,0000,0000,0000,,phạt đánh tôi 50 trượng
Dialogue: 0,1:13:34.17,1:13:35.73,Loithoai,,0000,0000,0000,,sau đó tôi sẽ đi trá hàng.
Dialogue: 0,1:13:36.23,1:13:38.89,Loithoai,,0000,0000,0000,,Tào Tháo nhận được thư hàng nhiều lần,\Nhắn nhất định sẽ tin tưởng tôi.
Dialogue: 0,1:13:39.36,1:13:42.71,Loithoai,,0000,0000,0000,,Khi đó tôi đưa hỏa thuyền của mình\Nnhất định sẽ tiếp cận được bọn chúng.
Dialogue: 0,1:13:43.24,1:13:46.49,Loithoai,,0000,0000,0000,,Sao khi bị dùng hình máu me be bét rồi ngài\Ncòn có thể dẫn binh đi đánh nhau được sao?
Dialogue: 0,1:13:47.57,1:13:50.27,Loithoai,,0000,0000,0000,,Ta sẽ không bao giờ dùng cách đó\Nvới một vị tướng quân như ngài đâu.
Dialogue: 0,1:13:51.09,1:13:52.85,Loithoai,,0000,0000,0000,,Đừng tự đánh mất lòng tin.
Dialogue: 0,1:13:53.27,1:13:54.39,Loithoai,,0000,0000,0000,,Gió đông nhất định sẽ tới.
Dialogue: 0,1:13:54.72,1:13:55.76,Loithoai,,0000,0000,0000,,Đại nhân.
Dialogue: 0,1:13:56.34,1:13:58.40,Loithoai,,0000,0000,0000,,Không ổn rồi.
Dialogue: 0,1:13:58.96,1:14:01.29,Loithoai,,0000,0000,0000,,Phu nhân, phu nhân không thấy đâu cả.
Dialogue: 0,1:14:24.61,1:14:27.35,Loithoai,,0000,0000,0000,,Chu lang, lúc chàng đang xem thư này.
Dialogue: 0,1:14:29.52,1:14:32.43,Loithoai,,0000,0000,0000,,Thiếp đã trên đường tới\Ndoanh trại của quân Tào rồi.
Dialogue: 0,1:14:34.30,1:14:36.20,Loithoai,,0000,0000,0000,,Thiếp sẽ thật thận trọng,
Dialogue: 0,1:14:36.68,1:14:37.64,Loithoai,,0000,0000,0000,,lựa thời cơ hành sự.
Dialogue: 0,1:14:40.46,1:14:43.01,Loithoai,,0000,0000,0000,,Nhưng thiếp vẫn mong gió Đông\Nsẽ đến sớm hơn một chút.
Dialogue: 0,1:14:43.84,1:14:47.84,Loithoai,,0000,0000,0000,,Chỉ cần có niềm tin, sẽ có hi vọng.
Dialogue: 0,1:14:51.71,1:14:52.87,Loithoai,,0000,0000,0000,,Trường Giang nước xanh.
Dialogue: 0,1:14:54.43,1:14:56.31,Loithoai,,0000,0000,0000,,Sơn sắc thường lục.
Dialogue: 0,1:14:57.08,1:14:59.38,Loithoai,,0000,0000,0000,,Có đàn chim bay về tổ.
Dialogue: 0,1:15:00.09,1:15:02.22,Loithoai,,0000,0000,0000,,Có ngư dân cất cao tiếng hát khi màn đêm về.
Dialogue: 0,1:15:03.99,1:15:05.51,Loithoai,,0000,0000,0000,,Đây chính là nhà của chúng ta.
Dialogue: 0,1:15:06.22,1:15:08.34,Loithoai,,0000,0000,0000,,Vùng đất tươi đẹp của hai ta.
Dialogue: 0,1:15:10.26,1:15:13.46,Loithoai,,0000,0000,0000,,Mọi người đều không màng\Ntới sinh tử để bảo vệ nó.
Dialogue: 0,1:15:14.58,1:15:17.70,Loithoai,,0000,0000,0000,,Thiếp chẳng thể nào thảnh thơi\Nngồi một chỗ như vậy được.
Dialogue: 0,1:15:41.60,1:15:44.68,Loithoai,,0000,0000,0000,,Thiếp đã mang thai\NBình An 3 tháng nay rồi.
Dialogue: 0,1:15:45.15,1:15:47.44,Loithoai,,0000,0000,0000,,Thiếp không nói rõ cho chàng biết
Dialogue: 0,1:15:48.22,1:15:50.21,Loithoai,,0000,0000,0000,,sợ chàng phân tâm.
Dialogue: 0,1:15:51.43,1:15:55.93,Loithoai,,0000,0000,0000,,Thiếp muốn mình không chỉ\Nchăm sóc đứa con của hai ta,
Dialogue: 0,1:15:57.07,1:15:59.49,Loithoai,,0000,0000,0000,,mà bỏ quên những đứa trẻ\Ncủa Đông Ngô này.
Dialogue: 0,1:16:00.66,1:16:03.86,Loithoai,,0000,0000,0000,,Thiếp hi vọng được dõi theo\Nchúng lớn lên từng ngày
Dialogue: 0,1:16:04.39,1:16:07.59,Loithoai,,0000,0000,0000,,tại một mảnh đất thanh bình mà\Nchúng có thể tự do vui đùa bên sông
Dialogue: 0,1:16:08.52,1:16:11.84,Loithoai,,0000,0000,0000,,đùa nghịch trong làn nước,\Nngắt những bông hoa
Dialogue: 0,1:16:12.30,1:16:15.47,Loithoai,,0000,0000,0000,,và cấy mạ trên ruộng lúa.
Dialogue: 0,1:16:15.95,1:16:18.31,Loithoai,,0000,0000,0000,,Nãi tử Bình An
Dialogue: 0,1:16:18.70,1:16:21.23,Loithoai,,0000,0000,0000,,DỰNG\N(Có thai)
Dialogue: 0,1:16:22.21,1:16:24.44,Loithoai,,0000,0000,0000,,Chàng không cần lo lắng
Dialogue: 0,1:16:25.50,1:16:28.50,Loithoai,,0000,0000,0000,,Thiếp sẽ đem Bình An trở về.
Dialogue: 0,1:17:06.77,1:17:09.56,Loithoai,,0000,0000,0000,,Một vài món ăn truyền thống rất có ý nghĩa.
Dialogue: 0,1:17:10.35,1:17:13.02,Loithoai,,0000,0000,0000,,Chẳng hạn như bột gạo với đường
Dialogue: 0,1:17:13.45,1:17:17.08,Loithoai,,0000,0000,0000,,khi hòa trộn lại với nhau\Nmang ý nghĩa hợp gia đoàn viên.
Dialogue: 0,1:17:25.93,1:17:28.20,Loithoai,,0000,0000,0000,,Nào nào nào, mau đưa xe hàng tới đây.
Dialogue: 0,1:17:28.58,1:17:31.75,Loithoai,,0000,0000,0000,,Mau mau đem bánh trôi đến cho\Nmọi người, ai ai cũng đều có cả.
Dialogue: 0,1:17:32.68,1:17:34.82,Loithoai,,0000,0000,0000,,Hôm nay là tiết đông chí, cả nhà đoàn viên.
Dialogue: 0,1:17:35.16,1:17:37.03,Loithoai,,0000,0000,0000,,Mọi người cùng ăn bánh trôi đi nào.
Dialogue: 0,1:17:37.84,1:17:40.91,Loithoai,,0000,0000,0000,,Mỗi người 2 hoặc 3 viên,\Nmọi người đều phải ăn đấy!
Dialogue: 0,1:17:43.44,1:17:44.70,Loithoai,,0000,0000,0000,,Mọi người qua đây cả đi.
Dialogue: 0,1:17:45.03,1:17:47.39,Loithoai,,0000,0000,0000,,Mỗi người 2 đến 3 viên,\Nmọi người đều phải ăn cả đấy!
Dialogue: 0,1:17:47.80,1:17:49.53,Loithoai,,0000,0000,0000,,Qua đây qua đây!
Dialogue: 0,1:17:50.57,1:17:51.55,Loithoai,,0000,0000,0000,,Cầm lấy bát này.
Dialogue: 0,1:18:07.45,1:18:09.87,Loithoai,,0000,0000,0000,,Tam đệ, nặn như vậy to quá rồi.
Dialogue: 0,1:18:10.27,1:18:12.33,Loithoai,,0000,0000,0000,,Muốn người ta chết nghẹn chắc?
Dialogue: 0,1:18:12.70,1:18:15.29,Loithoai,,0000,0000,0000,,Đệ đang vui, đệ muốn thế.
Dialogue: 0,1:19:22.63,1:19:23.31,Loithoai,,0000,0000,0000,,Tử Long à.
Dialogue: 0,1:19:23.93,1:19:26.60,Loithoai,,0000,0000,0000,,Vết thương lúc luyện binh\Ncủa ngươi khỏi chưa?
Dialogue: 0,1:19:27.03,1:19:30.31,Loithoai,,0000,0000,0000,,Chỉ hận không thể ra trận giết địch,\Nvết thương nhỏ đó có đáng gì.
Dialogue: 0,1:19:44.82,1:19:47.67,Loithoai,,0000,0000,0000,,Nhị đệ, đã đọc kinh Xuân Thu\Nđược mấy lần rồi?
Dialogue: 0,1:19:49.14,1:19:51.97,Loithoai,,0000,0000,0000,,Đọc sách bao nhiêu lần mà\Nkhông hiểu được nghĩa khí,
Dialogue: 0,1:19:52.44,1:19:53.93,Loithoai,,0000,0000,0000,,chẳng bằng không đọc còn hơn.
Dialogue: 0,1:20:16.89,1:20:20.56,Loithoai,,0000,0000,0000,,Đại ca, chúng ta ngồi\Ntrong này làm cái gì?
Dialogue: 0,1:20:21.70,1:20:22.19,Loithoai,,0000,0000,0000,,Đợi.
Dialogue: 0,1:20:23.51,1:20:24.89,Loithoai,,0000,0000,0000,,Đợi cái gì?
Dialogue: 0,1:20:25.58,1:20:27.04,Loithoai,,0000,0000,0000,,Đợi Khổng Minh.
Dialogue: 0,1:20:27.69,1:20:30.43,Loithoai,,0000,0000,0000,,Đợi cậu ta về rồi,\Nchúng ta có thể về nhà ngay.
Dialogue: 0,1:20:31.66,1:20:33.85,Loithoai,,0000,0000,0000,,Chúng ta vẫn còn nhà sao?
Dialogue: 0,1:20:34.58,1:20:37.57,Loithoai,,0000,0000,0000,,Những người con của Đông Ngô, nghe đây.
Dialogue: 0,1:20:38.21,1:20:40.09,Loithoai,,0000,0000,0000,,Hôm này là Đông Tiết.
Dialogue: 0,1:20:41.21,1:20:44.19,Loithoai,,0000,0000,0000,,Đáng ra nên là ngày nhất gia đoàn viên.
Dialogue: 0,1:20:44.80,1:20:48.67,Loithoai,,0000,0000,0000,,Thế nhưng bọn địch lại bao vây\Nquê hương của chúng ta.
Dialogue: 0,1:20:49.77,1:20:52.01,Loithoai,,0000,0000,0000,,Chúng ta ăn uống làm sao đây?
Dialogue: 0,1:20:52.83,1:20:56.59,Loithoai,,0000,0000,0000,,Nghe ta nói, chúng ta đi đuổi cổ\Nbọn chúng ra khỏi nơi đây,
Dialogue: 0,1:20:57.22,1:21:00.22,Loithoai,,0000,0000,0000,,Sau đó lại cùng ngồi xuống ăn\Nmột bữa cơm đoàn viên đích thực.
Dialogue: 0,1:21:00.34,1:21:01.47,Loithoai,,0000,0000,0000,,Được.
Dialogue: 0,1:21:06.35,1:21:07.26,Loithoai,,0000,0000,0000,,Đại ca.
Dialogue: 0,1:21:08.02,1:21:09.64,Loithoai,,0000,0000,0000,,Có những lúc đệ không đồng ý\Nvới cách làm của huynh,
Dialogue: 0,1:21:09.96,1:21:12.90,Loithoai,,0000,0000,0000,,nhưng đệ đối với huynh chưa từng\Ncó một câu hai lời
Dialogue: 0,1:21:14.18,1:21:18.24,Loithoai,,0000,0000,0000,,Nhưng lần này là chọn lựa\Ngiữa thị phi khúc trực đó.
Dialogue: 0,1:21:18.70,1:21:20.17,Loithoai,,0000,0000,0000,,Ta chỉ muốn tốt cho mọi người thôi
Dialogue: 0,1:21:20.71,1:21:23.36,Loithoai,,0000,0000,0000,,vậy mà các người lại nói rằng\Nta bất nhân bất nghĩa.
Dialogue: 0,1:21:23.78,1:21:26.48,Loithoai,,0000,0000,0000,,Các người theo ta bao nhiêu lâu\Nnhư vậy là vì cái gì?
Dialogue: 0,1:21:26.86,1:21:29.02,Loithoai,,0000,0000,0000,,Ta chỉ muốn đảm bảo tính mệnh\Ncho mọi người mà thôi.
Dialogue: 0,1:21:53.02,1:21:57.54,Loithoai,,0000,0000,0000,,Có những thứ còn\Nđáng quý hơn cả tính mệnh.
Dialogue: 0,1:23:00.82,1:23:05.57,Loithoai,,0000,0000,0000,,Chu đô đốc, hi vọng kế hoạch\Ncủa ngài có thể thành công.
Dialogue: 0,1:23:06.28,1:23:08.46,Loithoai,,0000,0000,0000,,Chúng tôi sẽ đánh thật tốt trận này.
Dialogue: 0,1:23:30.43,1:23:31.85,Loithoai,,0000,0000,0000,,Phu nhân của Chu Du tới.
Dialogue: 0,1:23:32.26,1:23:34.00,Loithoai,,0000,0000,0000,,Chỉ có một mình thôi.
Dialogue: 0,1:25:31.74,1:25:32.71,Loithoai,,0000,0000,0000,,Trọng huynh,
Dialogue: 0,1:25:33.75,1:25:35.92,Loithoai,,0000,0000,0000,,kế sách tiến công nhất định phải thay đổi.
Dialogue: 0,1:25:36.81,1:25:38.28,Loithoai,,0000,0000,0000,,Để đảm bảo an nguy cho nhị tẩu,
Dialogue: 0,1:25:38.67,1:25:40.19,Loithoai,,0000,0000,0000,,ta sẽ đưa những kị binh tốt nhất
Dialogue: 0,1:25:40.61,1:25:42.79,Loithoai,,0000,0000,0000,,hướng đến phía sau doanh trại\Nquân Tào để giải cứu tẩu ấy.
Dialogue: 0,1:25:43.74,1:25:44.79,Loithoai,,0000,0000,0000,,Không thể được.
Dialogue: 0,1:25:45.82,1:25:49.13,Loithoai,,0000,0000,0000,,Chúng ta cần tập trung lực lượng trọng yếu\Nđể cống kích chính diện quân Tào.
Dialogue: 0,1:25:49.57,1:25:51.44,Loithoai,,0000,0000,0000,,Không thể nào phân chia binh lực được.
Dialogue: 0,1:25:52.67,1:25:55.80,Loithoai,,0000,0000,0000,,Công Cẩn, hiện giờ chỉ\Nmình chúng ta giao chiến,
Dialogue: 0,1:25:56.39,1:25:57.70,Loithoai,,0000,0000,0000,,nếu không có đồng minh hỗ trợ,
Dialogue: 0,1:25:58.08,1:26:00.50,Loithoai,,0000,0000,0000,,tôi e rằng cơ hội chiến thắng rất mỏng manh.
Dialogue: 0,1:26:01.57,1:26:03.35,Loithoai,,0000,0000,0000,,Ta chưa bao giờ từ bỏ niềm tin của mình.
Dialogue: 0,1:26:04.12,1:26:06.64,Loithoai,,0000,0000,0000,,Nhưng chiến thắng không thể\Nchỉ dựa vào niềm tin được.
Dialogue: 0,1:26:13.16,1:26:14.46,Loithoai,,0000,0000,0000,,Khi nào thì gió Đông sẽ tới?
Dialogue: 0,1:26:14.91,1:26:17.33,Loithoai,,0000,0000,0000,,Tôi đã quan sát thêm lần nữa,\Nvào giờ Sửu đêm nay
Dialogue: 0,1:26:17.73,1:26:19.14,Loithoai,,0000,0000,0000,,hướng gió sẽ thay đổi.
Dialogue: 0,1:26:19.25,1:26:21.40,Loithoai,,0000,0000,0000,,Nếu như ngài còn tin lời của người bạn này,
Dialogue: 0,1:26:21.88,1:26:24.25,Loithoai,,0000,0000,0000,,đến lúc đó hãy thả đèn truyền tin được chứ?
Dialogue: 0,1:26:28.24,1:26:30.68,Loithoai,,0000,0000,0000,,Nếu như có chuyện không hay\Nxảy đến với tẩu tẩu,
Dialogue: 0,1:26:31.16,1:26:33.75,Loithoai,,0000,0000,0000,,huynh vẫn sẽ tiếp tục\Ncuộc chiến quân tử chứ?
Dialogue: 0,1:26:52.19,1:26:54.61,Loithoai,,0000,0000,0000,,Ta đã một lần trông thấy nàng\Nở hậu viện tại nhà nàng.
Dialogue: 0,1:26:55.34,1:26:59.21,Loithoai,,0000,0000,0000,,Khi đó nàng vẫn còn nhỏ,\Nnhưng đã là một mỹ nhân rồi.
Dialogue: 0,1:27:00.08,1:27:01.60,Loithoai,,0000,0000,0000,,khi đó nàng đang học nghệ thuật trà đạo
Dialogue: 0,1:27:01.99,1:27:04.86,Loithoai,,0000,0000,0000,,và còn thích bắt bướm nữa.
Dialogue: 0,1:27:23.04,1:27:25.57,Loithoai,,0000,0000,0000,,Ngài vì tôi mà đánh trận này sao?
Dialogue: 0,1:27:26.12,1:27:27.54,Loithoai,,0000,0000,0000,,Nàng thật sự tin là thế sao?
Dialogue: 0,1:27:28.77,1:27:31.01,Loithoai,,0000,0000,0000,,Phụ thân tôi trước kia\Nrất hay tán dương ngài.
Dialogue: 0,1:27:32.13,1:27:34.92,Loithoai,,0000,0000,0000,,Khi đó Tào Tháo là người rất\Nchính nghĩa, rất có lý tưởng
Dialogue: 0,1:27:35.44,1:27:37.26,Loithoai,,0000,0000,0000,,đối với Hán triều rất mực trung thành
Dialogue: 0,1:27:37.68,1:27:39.46,Loithoai,,0000,0000,0000,,Đúng là một thanh niên anh hùng.
Dialogue: 0,1:27:40.87,1:27:44.59,Loithoai,,0000,0000,0000,,Nếu không làm sao giờ\Ncó thể làm thừa tướng được?
Dialogue: 0,1:27:46.62,1:27:50.05,Loithoai,,0000,0000,0000,,Làm thừa tướng nghĩa là phải\Ngiết quá nhiều người vậy sao?
Dialogue: 0,1:27:54.52,1:27:57.96,Loithoai,,0000,0000,0000,,Nghe nói binh sĩ Đông Ngô đổ bệnh rất nhiều.
Dialogue: 0,1:27:58.18,1:27:59.29,Loithoai,,0000,0000,0000,,Đúng vậy.
Dialogue: 0,1:27:59.96,1:28:02.32,Loithoai,,0000,0000,0000,,Nghe nói Lưu Bị đã bỏ đi mất
Dialogue: 0,1:28:02.86,1:28:04.48,Loithoai,,0000,0000,0000,,liên minh của chúng cũng tan rã rồi.
Dialogue: 0,1:28:05.91,1:28:06.78,Loithoai,,0000,0000,0000,,Đúng vậy.
Dialogue: 0,1:28:08.08,1:28:12.12,Loithoai,,0000,0000,0000,,Gã Chu Du đó không phải\Ngiờ đang rất đơn độc hay sao?
Dialogue: 0,1:28:19.22,1:28:20.42,Loithoai,,0000,0000,0000,,Hắn bảo nàng tới sao?
Dialogue: 0,1:28:20.85,1:28:22.40,Loithoai,,0000,0000,0000,,Là tôi tự đến.
Dialogue: 0,1:28:25.79,1:28:27.26,Loithoai,,0000,0000,0000,,Nàng tới có việc gì?
Dialogue: 0,1:28:28.13,1:28:30.23,Loithoai,,0000,0000,0000,,Thỉnh cầu ngài thu binh.
Dialogue: 0,1:28:31.52,1:28:33.04,Loithoai,,0000,0000,0000,,Ngài đã thắng rồi.
Dialogue: 0,1:28:33.73,1:28:35.26,Loithoai,,0000,0000,0000,,Cớ gì lại phải đánh nhau nữa?
Dialogue: 0,1:28:35.59,1:28:36.49,Loithoai,,0000,0000,0000,,Không.
Dialogue: 0,1:28:37.52,1:28:40.92,Loithoai,,0000,0000,0000,,Ta muốn gã Chu Du đó quỳ\Ntrước mặt ta xin đầu hàng.
Dialogue: 0,1:28:45.72,1:28:47.50,Loithoai,,0000,0000,0000,,Nàng làm thế này là ý gì?
Dialogue: 0,1:28:50.43,1:28:52.59,Loithoai,,0000,0000,0000,,Tôi không phải vì Chu lang,
Dialogue: 0,1:28:53.06,1:28:54.06,Loithoai,,0000,0000,0000,,mà chính là vì bách tính toàn thiên hạ.
Dialogue: 0,1:28:55.49,1:28:58.98,Loithoai,,0000,0000,0000,,Thỉnh cầu thừa tướng\Nhãy ngưng cuộc chiến này.
Dialogue: 0,1:28:59.87,1:29:02.18,Loithoai,,0000,0000,0000,,Nếu như ta không ưng thuận thì sao?
Dialogue: 0,1:29:09.47,1:29:10.54,Loithoai,,0000,0000,0000,,Đừng có làm loạn nữa.
Dialogue: 0,1:29:18.03,1:29:21.33,Loithoai,,0000,0000,0000,,Ta muốn nàng tận mắt chứng kiến\Nta đánh bại gã Chu Du đó như thế nào.
Dialogue: 0,1:30:03.89,1:30:06.12,Loithoai,,0000,0000,0000,,Đang nghĩ gì vậy?
Dialogue: 0,1:30:10.01,1:30:12.46,Loithoai,,0000,0000,0000,,Muội chợt nghĩ tới một gã ngốc.
Dialogue: 0,1:30:28.09,1:30:31.03,Loithoai,,0000,0000,0000,,Đây chính là thời thế tạo anh hùng.
Dialogue: 0,1:31:02.83,1:31:04.49,Loithoai,,0000,0000,0000,,Đợi đánh xong trận này,
Dialogue: 0,1:31:05.41,1:31:07.15,Loithoai,,0000,0000,0000,,ta sẽ tới uống trà mà nàng pha.
Dialogue: 0,1:31:07.58,1:31:09.25,Loithoai,,0000,0000,0000,,Uống trước một chén không được sao?
Dialogue: 0,1:31:09.76,1:31:11.51,Loithoai,,0000,0000,0000,,Bình tĩnh một chút.
Dialogue: 0,1:31:16.95,1:31:17.52,Loithoai,,0000,0000,0000,,Được.
Dialogue: 0,1:31:22.98,1:31:25.11,Loithoai,,0000,0000,0000,,Cho dù chẳng có ai coi trọng chúng ta.
Dialogue: 0,1:31:26.04,1:31:27.40,Loithoai,,0000,0000,0000,,Chẳng sao cả.
Dialogue: 0,1:31:27.86,1:31:29.73,Loithoai,,0000,0000,0000,,Chỉ cần bản thân chúng ta tự\Ncoi trọng bản thân mình là được.
Dialogue: 0,1:31:30.06,1:31:30.99,Loithoai,,0000,0000,0000,,Mọi người nghe đây.
Dialogue: 0,1:31:31.44,1:31:36.09,Loithoai,,0000,0000,0000,,Khi thuyền của chúng ta lao\Ntới thuyền trận của quân Tào,
Dialogue: 0,1:31:36.71,1:31:40.21,Loithoai,,0000,0000,0000,,Không thấy lửa, không ai\Nđược phép nhảy ra khỏi thuyền.
Dialogue: 0,1:31:40.66,1:31:41.76,Loithoai,,0000,0000,0000,,Rõ.
Dialogue: 0,1:31:42.30,1:31:43.59,Loithoai,,0000,0000,0000,,Mọi người còn vấn đề gì nữa không?
Dialogue: 0,1:31:43.90,1:31:44.85,Loithoai,,0000,0000,0000,,Không còn.
Dialogue: 0,1:31:45.25,1:31:46.48,Loithoai,,0000,0000,0000,,Tốt lắm.
Dialogue: 0,1:31:46.90,1:31:49.88,Loithoai,,0000,0000,0000,,Mọi người đã viết thư\Ncho gia đình mình chưa?
Dialogue: 0,1:31:50.00,1:31:51.01,Loithoai,,0000,0000,0000,,Viết rồi.
Dialogue: 0,1:32:26.61,1:32:27.53,Loithoai,,0000,0000,0000,,Đừng vội.
Dialogue: 0,1:32:27.88,1:32:30.45,Loithoai,,0000,0000,0000,,Đầu tiên hãy quan sát màu sắc của trà,\Nthưởng thức hương trà.
Dialogue: 0,1:32:30.82,1:32:31.73,Loithoai,,0000,0000,0000,,Được.
Dialogue: 0,1:32:55.41,1:32:56.92,Loithoai,,0000,0000,0000,,Thừa tướng tại sao vẫn chưa phát binh?
Dialogue: 0,1:32:57.22,1:32:58.43,Loithoai,,0000,0000,0000,,Hướng gió có biểu hiện lạ lắm.
Dialogue: 0,1:32:58.74,1:32:59.70,Loithoai,,0000,0000,0000,,Ta biết chứ.
Dialogue: 0,1:33:00.48,1:33:02.25,Loithoai,,0000,0000,0000,,Thừa tướng đang cùng với\Nngười đàn bà ở trong đó uống trà.
Dialogue: 0,1:33:02.33,1:33:04.06,Loithoai,,0000,0000,0000,,Uống trà?
Dialogue: 0,1:33:05.73,1:33:08.58,Loithoai,,0000,0000,0000,,Điều khó nhất khi pha trà là gì vậy?
Dialogue: 0,1:33:08.96,1:33:13.19,Loithoai,,0000,0000,0000,,Lá trà, lửa, nước, dụng cụ
Dialogue: 0,1:33:13.68,1:33:15.57,Loithoai,,0000,0000,0000,,đều cần lưu tâm.
Dialogue: 0,1:33:20.18,1:33:22.08,Loithoai,,0000,0000,0000,,Nhưng khó nhất chính là đun sôi nước.
Dialogue: 0,1:33:23.40,1:33:24.45,Loithoai,,0000,0000,0000,,Đun nước?
Dialogue: 0,1:33:26.92,1:33:28.34,Loithoai,,0000,0000,0000,,Mọi người nhìn xem.
Dialogue: 0,1:33:29.80,1:33:31.77,Loithoai,,0000,0000,0000,,Gió lặng rồi.
Dialogue: 0,1:33:42.40,1:33:45.33,Loithoai,,0000,0000,0000,,Sôi lần 1, sôi như mắt cá\Ncó thanh âm nho nhỏ.
Dialogue: 0,1:33:45.81,1:33:49.01,Loithoai,,0000,0000,0000,,Sôi lần 2, bên mép\Nphun trào như liên châu.
Dialogue: 0,1:33:49.58,1:33:50.83,Loithoai,,0000,0000,0000,,Đây chính là lúc dùng trà,\Ntrà phẩm tuyệt nhất.
Dialogue: 0,1:33:53.12,1:33:54.37,Loithoai,,0000,0000,0000,,Sôi lần 3, nước sôi như sóng trào.
Dialogue: 0,1:33:56.08,1:33:59.48,Loithoai,,0000,0000,0000,,Nếu còn đun nữa nước ngay lập tức\Nsẽ già mất, không thể uống được nữa.
Dialogue: 0,1:34:01.47,1:34:03.75,Loithoai,,0000,0000,0000,,Đô đốc, gió sắp sửa đổi chiều rồi.
Dialogue: 0,1:34:04.17,1:34:06.69,Loithoai,,0000,0000,0000,,Có thể hành động rồi.\NChúng ta còn chờ gì nữa đây?
Dialogue: 0,1:34:07.10,1:34:08.08,Loithoai,,0000,0000,0000,,Chờ đồng minh.
Dialogue: 0,1:34:08.41,1:34:09.38,Loithoai,,0000,0000,0000,,Chúng ta còn có đồng minh nào nữa chứ?
Dialogue: 0,1:34:14.61,1:34:16.15,Loithoai,,0000,0000,0000,,Lưu Bị đã lui binh mấy ngày trước rồi mà.
Dialogue: 0,1:34:16.54,1:34:18.70,Loithoai,,0000,0000,0000,,Đây chính là mưu kế\Ndo trọng huynh đã an bài.
Dialogue: 0,1:34:19.29,1:34:22.04,Loithoai,,0000,0000,0000,,Có những sự tình\Ncàng ít người biết càng tốt.
Dialogue: 0,1:34:26.17,1:34:27.29,Loithoai,,0000,0000,0000,,Tràn mất rồi. Tràn rồi.
Dialogue: 0,1:34:28.14,1:34:29.57,Loithoai,,0000,0000,0000,,Thừa tướng.
Dialogue: 0,1:34:30.20,1:34:31.45,Loithoai,,0000,0000,0000,,Ngài không thấy chính mình\Ntrong chén trà này sao?
Dialogue: 0,1:34:34.12,1:34:36.21,Loithoai,,0000,0000,0000,,Ngài cũng như chén trà đều đầy tràn cả.
Dialogue: 0,1:34:36.59,1:34:42.14,Loithoai,,0000,0000,0000,,Chính vì thế lời của người khác chẳng\Nthể nào chạm tới tâm can của ngài.
Dialogue: 0,1:34:42.79,1:34:45.58,Loithoai,,0000,0000,0000,,Ngài mang theo một dã tâm\Nlớn lao tràn đầy tới Xích Bích.
Dialogue: 0,1:34:46.02,1:34:46.74,Loithoai,,0000,0000,0000,,Ắt hẳn rồi sẽ có người hắt đổ dã tâm đó đi.
Dialogue: 0,1:35:08.76,1:35:10.08,Loithoai,,0000,0000,0000,,Thừa tướng.
Dialogue: 0,1:35:10.53,1:35:11.49,Loithoai,,0000,0000,0000,,Ngài căn bản không hiểu về nghệ thuật trà.
Dialogue: 0,1:35:20.20,1:35:23.20,Loithoai,,0000,0000,0000,,Đô đốc, hãy cho chúng tôi biết\Ngiờ phải làm gì?
Dialogue: 0,1:35:23.33,1:35:24.56,Loithoai,,0000,0000,0000,,Vào canh hai giờ Sửu,
Dialogue: 0,1:35:24.95,1:35:27.57,Loithoai,,0000,0000,0000,,quân Hoàng Cái dẫn 10 hỏa thuyền\Nxung kích thuyền trận của quân Tào
Dialogue: 0,1:35:27.99,1:35:30.02,Loithoai,,0000,0000,0000,,tới chính diện châm lửa.
Dialogue: 0,1:35:31.79,1:35:32.98,Loithoai,,0000,0000,0000,,Thuyền trận bị phá
Dialogue: 0,1:35:33.41,1:35:34.66,Loithoai,,0000,0000,0000,,Triệu Tử Long sẽ nhanh chóng tiến công\Nvào phía Đông doanh trại quân Tào
Dialogue: 0,1:35:36.38,1:35:38.17,Loithoai,,0000,0000,0000,,chuyển hướng chú ý của quân địch.
Dialogue: 0,1:35:40.58,1:35:43.41,Loithoai,,0000,0000,0000,,Lưu Dự Châu, Quan Vân Trường\Nvà Trương Dực Đức
Dialogue: 0,1:35:43.89,1:35:47.03,Loithoai,,0000,0000,0000,,sẽ dùng trọng binh công kích\Ncổng phía Tây doanh trại quân Tào.
Dialogue: 0,1:35:59.10,1:36:00.36,Loithoai,,0000,0000,0000,,Hoa Đà!
Dialogue: 0,1:36:02.98,1:36:03.96,Loithoai,,0000,0000,0000,,Thừa tướng.
Dialogue: 0,1:36:04.44,1:36:05.98,Loithoai,,0000,0000,0000,,Gọi Hoa Đà tới đây.\NTa đau đầu quá.
Dialogue: 0,1:36:06.37,1:36:08.87,Loithoai,,0000,0000,0000,,Thừa tướng, Hoa Đà đã đi rồi.
Dialogue: 0,1:36:10.31,1:36:11.68,Loithoai,,0000,0000,0000,,Ngươi nói gì?
Dialogue: 0,1:36:12.22,1:36:13.84,Loithoai,,0000,0000,0000,,Hoa Đà đã đi rồi.
Dialogue: 0,1:37:19.61,1:37:20.77,Loithoai,,0000,0000,0000,,Xuất phát!
Dialogue: 0,1:37:32.86,1:37:33.79,Loithoai,,0000,0000,0000,,Đây là gió gì vậy?
Dialogue: 0,1:37:34.16,1:37:35.49,Loithoai,,0000,0000,0000,,Thừa tướng, là gió Đông đó.
Dialogue: 0,1:37:35.81,1:37:37.72,Loithoai,,0000,0000,0000,,Tại sao gió lại đổi chiều được?\NKhông thể nào!
Dialogue: 0,1:37:43.46,1:37:45.11,Loithoai,,0000,0000,0000,,Phía trước có thuyền địch!
Dialogue: 0,1:37:46.18,1:37:47.78,Loithoai,,0000,0000,0000,,Tướng quân, có thuyền địch!
Dialogue: 0,1:37:48.17,1:37:49.17,Loithoai,,0000,0000,0000,,Mau!\NTách thuyền ra!
Dialogue: 0,1:37:49.50,1:37:50.45,Loithoai,,0000,0000,0000,,Tách thuyền!
Dialogue: 0,1:38:02.49,1:38:03.24,Loithoai,,0000,0000,0000,,Kia là cái gì?
Dialogue: 0,1:38:03.58,1:38:04.83,Loithoai,,0000,0000,0000,,Thừa tướng, đó là đèn của bọn chúng!
Dialogue: 0,1:38:09.04,1:38:10.29,Loithoai,,0000,0000,0000,,Tiến lên!
Dialogue: 0,1:38:17.27,1:38:18.50,Loithoai,,0000,0000,0000,,Đô đốc.
Dialogue: 0,1:38:19.06,1:38:20.51,Loithoai,,0000,0000,0000,,Tôi đã làm hết khả năng của mình rồi.
Dialogue: 0,1:38:20.98,1:38:24.75,Loithoai,,0000,0000,0000,,Tam phân thiên hạ, chính là hôm nay.
Dialogue: 0,1:38:28.66,1:38:29.94,Loithoai,,0000,0000,0000,,Mọi chuyện không được\Nđể xảy ra sơ xuất nhỏ nào.
Dialogue: 0,1:38:30.29,1:38:31.38,Loithoai,,0000,0000,0000,,Khổng Minh huynh.
Dialogue: 0,1:38:31.81,1:38:33.52,Loithoai,,0000,0000,0000,,Ta biết huynh không có lừa mọi người mà.
Dialogue: 0,1:38:41.60,1:38:42.27,Loithoai,,0000,0000,0000,,Báo!
Dialogue: 0,1:38:42.60,1:38:43.97,Loithoai,,0000,0000,0000,,Trên sông có thuyền, bọn chúng\Nđang tiến tới đây nhanh lắm.
Dialogue: 0,1:38:44.37,1:38:45.24,Loithoai,,0000,0000,0000,,Truyền lệnh mau tách thuyền!
Dialogue: 0,1:38:45.55,1:38:47.15,Loithoai,,0000,0000,0000,,Bẩm thừa tướng,\Nđang tiến hành tách thuyền rồi.
Dialogue: 0,1:39:00.12,1:39:01.30,Loithoai,,0000,0000,0000,,Châm lửa!
Dialogue: 0,1:39:09.65,1:39:10.27,Loithoai,,0000,0000,0000,,Tiến lên phía trước!
Dialogue: 0,1:39:19.95,1:39:21.58,Loithoai,,0000,0000,0000,,Mau\NMau qua đây!
Dialogue: 0,1:39:47.83,1:39:49.48,Loithoai,,0000,0000,0000,,Hết tốc lực tiến lên phía trước!
Dialogue: 0,1:40:36.37,1:40:37.38,Loithoai,,0000,0000,0000,,Mau tách thuyền ra!
Dialogue: 0,1:40:39.27,1:40:40.34,Loithoai,,0000,0000,0000,,Mau lên!
Dialogue: 0,1:41:40.59,1:41:43.12,Loithoai,,0000,0000,0000,,Rút lui!\NBảo vệ cổng chính!
Dialogue: 0,1:41:43.59,1:41:48.25,Loithoai,,0000,0000,0000,,Tiến lên! Thẳng hướng\Nthuyền chính của Tào Tháo!
Dialogue: 0,1:43:37.48,1:43:39.18,Loithoai,,0000,0000,0000,,Động như lôi chấn.
Dialogue: 0,1:44:15.01,1:44:16.11,Loithoai,,0000,0000,0000,,Xung phong!
Dialogue: 0,1:44:18.19,1:44:19.08,Loithoai,,0000,0000,0000,,Được rồi!
Dialogue: 0,1:44:19.38,1:44:20.32,Loithoai,,0000,0000,0000,,Xung phong!
Dialogue: 0,1:44:41.84,1:44:43.08,Loithoai,,0000,0000,0000,,Mọi người theo ta!
Dialogue: 0,1:45:05.57,1:45:06.51,Loithoai,,0000,0000,0000,,Phóng tiễn!
Dialogue: 0,1:45:20.25,1:45:21.96,Loithoai,,0000,0000,0000,,Bắn đá!
Dialogue: 0,1:45:38.03,1:45:39.54,Loithoai,,0000,0000,0000,,Địch quân đang tới.\NMau!
Dialogue: 0,1:45:40.43,1:45:41.91,Loithoai,,0000,0000,0000,,Nhìn kìa, phía bên kia!
Dialogue: 0,1:46:03.78,1:46:05.83,Loithoai,,0000,0000,0000,,Thừa tướng, đại sự không ổn rồi!
Dialogue: 0,1:46:06.27,1:46:07.20,Loithoai,,0000,0000,0000,,Bọn chúng tấn công tới đại doanh rồi.
Dialogue: 0,1:46:07.48,1:46:08.43,Loithoai,,0000,0000,0000,,Ngươi sợ gì chứ?
Dialogue: 0,1:46:10.44,1:46:12.03,Loithoai,,0000,0000,0000,,Đừng quên ngươi là một đại tướng!
Dialogue: 0,1:46:12.18,1:46:12.74,Loithoai,,0000,0000,0000,,Tôi...
Dialogue: 0,1:47:18.36,1:47:19.77,Loithoai,,0000,0000,0000,,Cố gắng chịu đựng!
Dialogue: 0,1:47:40.03,1:47:41.78,Loithoai,,0000,0000,0000,,Mau!\NTăng viện cho tam tướng quân!
Dialogue: 0,1:48:06.24,1:48:07.42,Loithoai,,0000,0000,0000,,Thừa tướng!
Dialogue: 0,1:48:07.85,1:48:09.53,Loithoai,,0000,0000,0000,,Hậu phương của chúng ta bất ngờ bị tập kích.
Dialogue: 0,1:48:10.02,1:48:11.85,Loithoai,,0000,0000,0000,,Là quân của Lưu Bị.
Dialogue: 0,1:48:12.71,1:48:13.89,Loithoai,,0000,0000,0000,,Lưu Bị?
Dialogue: 0,1:48:16.24,1:48:17.29,Loithoai,,0000,0000,0000,,Mau thả ngựa của địch ra!
Dialogue: 0,1:48:18.06,1:48:18.86,Loithoai,,0000,0000,0000,,Kéo!
Dialogue: 0,1:48:35.60,1:48:36.44,Loithoai,,0000,0000,0000,,Cam tướng quân!
Dialogue: 0,1:48:36.75,1:48:38.76,Loithoai,,0000,0000,0000,,Tôi cần giữ binh lực để\Nthực hiện công kích lần 2.
Dialogue: 0,1:48:42.50,1:48:43.85,Loithoai,,0000,0000,0000,,Ngài cần phải công phá được cánh cổng này
Dialogue: 0,1:48:44.18,1:48:45.54,Loithoai,,0000,0000,0000,,thì chúng ta mới có thể mau chóng\Nhội binh với Lưu Bị được.
Dialogue: 0,1:48:45.86,1:48:46.64,Loithoai,,0000,0000,0000,,bằng không mọi chuyện sẽ hỏng hết.
Dialogue: 0,1:48:46.90,1:48:47.49,Loithoai,,0000,0000,0000,,Không vấn đề!
Dialogue: 0,1:48:47.76,1:48:48.50,Loithoai,,0000,0000,0000,,Cẩn thận nhé!
Dialogue: 0,1:49:20.56,1:49:21.26,Loithoai,,0000,0000,0000,,Dừng lại!
Dialogue: 0,1:49:24.86,1:49:26.26,Loithoai,,0000,0000,0000,,Dọn dẹp chướng ngại!
Dialogue: 0,1:49:29.94,1:49:30.98,Loithoai,,0000,0000,0000,,Phóng tiễn!
Dialogue: 0,1:49:52.75,1:49:53.61,Loithoai,,0000,0000,0000,,Lên!
Dialogue: 0,1:49:54.32,1:49:55.70,Loithoai,,0000,0000,0000,,Công phá cổng lớn!
Dialogue: 0,1:50:12.57,1:50:13.44,Loithoai,,0000,0000,0000,,A Ngưu!
Dialogue: 0,1:50:15.36,1:50:16.67,Loithoai,,0000,0000,0000,,Nằm tạm ở đây nhé!
Dialogue: 0,1:50:27.34,1:50:28.44,Loithoai,,0000,0000,0000,,Cam tướng quân.
Dialogue: 0,1:50:28.31,1:50:30.64,Loithoai,,0000,0000,0000,,Hỏa lực vẫn chưa đủ mạnh.
Dialogue: 0,1:50:31.64,1:50:33.74,Loithoai,,0000,0000,0000,,Lửa cháy tới nơi rồi!\NChạy mau!
Dialogue: 0,1:50:34.71,1:50:35.95,Loithoai,,0000,0000,0000,,Mấy khối thuôc nổ mang qua cả đây!
Dialogue: 0,1:50:36.26,1:50:37.20,Loithoai,,0000,0000,0000,,Còn có 3 cái thôi.
Dialogue: 0,1:50:38.17,1:50:40.91,Loithoai,,0000,0000,0000,,Quyết không rút lui!\NKhông đầu hàng!
Dialogue: 0,1:50:57.26,1:50:59.05,Loithoai,,0000,0000,0000,,Cái này tốt lắm này!
Dialogue: 0,1:51:21.33,1:51:22.92,Loithoai,,0000,0000,0000,,Tổng công kích!
Dialogue: 0,1:52:31.51,1:52:32.18,Loithoai,,0000,0000,0000,,Phóng tiễn!
Dialogue: 0,1:53:50.86,1:53:51.85,Loithoai,,0000,0000,0000,,Thừa tướng!
Dialogue: 0,1:53:52.21,1:53:53.43,Loithoai,,0000,0000,0000,,Toàn bộ thủy quân đã bị tiêu diệt rồi.
Dialogue: 0,1:53:53.84,1:53:56.89,Loithoai,,0000,0000,0000,,Đông, Nam, Tây cả 3 mặt,\Nchúng ta đều bị công phá rồi.
Dialogue: 0,1:54:05.79,1:54:09.48,Loithoai,,0000,0000,0000,,Bọn chúng chắc cũng bị lửa thiêu rồi.
Dialogue: 0,1:54:10.32,1:54:11.25,Loithoai,,0000,0000,0000,,Đúng vậy, thưa thừa tướng.
Dialogue: 0,1:54:11.98,1:54:13.85,Loithoai,,0000,0000,0000,,Phía dưới chẳng còn động tĩnh gì nữa rồi.
Dialogue: 0,1:54:27.68,1:54:29.26,Loithoai,,0000,0000,0000,,Ngọn lửa dữ dội quá.
Dialogue: 0,1:54:29.69,1:54:30.87,Loithoai,,0000,0000,0000,,Mọi người phải tiến lui cùng nhau.
Dialogue: 0,1:54:31.23,1:54:32.95,Loithoai,,0000,0000,0000,,Dù thế nào cũng không được tách nhau ra.
Dialogue: 0,1:54:41.67,1:54:42.96,Loithoai,,0000,0000,0000,,Thừa tướng.
Dialogue: 0,1:54:44.90,1:54:46.39,Loithoai,,0000,0000,0000,,Chúng ta vẫn còn lui binh được.
Dialogue: 0,1:54:46.77,1:54:47.72,Loithoai,,0000,0000,0000,,Lui binh?
Dialogue: 0,1:54:48.58,1:54:50.99,Loithoai,,0000,0000,0000,,Chúng ta vẫn còn có đội\Nkỵ binh tinh nhuệ kia mà.
Dialogue: 0,1:54:51.44,1:54:53.29,Loithoai,,0000,0000,0000,,Bọn họ đều tình nguyện\Ntrao cả tính mạng cho ta mà.
Dialogue: 0,1:54:56.59,1:54:58.63,Loithoai,,0000,0000,0000,,Cho dù chỉ còn một binh\Nmột tốt cũng phải chiến đấu.
Dialogue: 0,1:54:59.10,1:55:00.56,Loithoai,,0000,0000,0000,,Đây là quân lệnh.
Dialogue: 0,1:55:07.20,1:55:09.85,Loithoai,,0000,0000,0000,,Tập trung toàn lực!\NXông lên phía trước!
Dialogue: 0,1:55:27.60,1:55:28.79,Loithoai,,0000,0000,0000,,Giỏi thì tới đây, Tào tặc!
Dialogue: 0,1:55:29.11,1:55:29.92,Loithoai,,0000,0000,0000,,Tướng quân!
Dialogue: 0,1:56:32.77,1:56:34.54,Loithoai,,0000,0000,0000,,Huynh đệ, ngươi cần cố gắng gượng.
Dialogue: 0,1:56:34.87,1:56:35.66,Loithoai,,0000,0000,0000,,Nhị ca.
Dialogue: 0,1:56:36.00,1:56:37.28,Loithoai,,0000,0000,0000,,Tam đệ, đại ca đâu.
Dialogue: 0,1:56:47.78,1:56:49.01,Loithoai,,0000,0000,0000,,Gia tăng tốc lực tiến lên!
Dialogue: 0,1:57:03.20,1:57:04.29,Loithoai,,0000,0000,0000,,Mở ra!
Dialogue: 0,1:58:02.43,1:58:03.45,Loithoai,,0000,0000,0000,,Ném thương!
Dialogue: 0,1:58:14.60,1:58:15.53,Loithoai,,0000,0000,0000,,Lên!
Dialogue: 0,1:58:15.88,1:58:19.25,Loithoai,,0000,0000,0000,,Ném đá giết hết bọn chúng đi!
Dialogue: 0,1:58:30.92,1:58:31.84,Loithoai,,0000,0000,0000,,Huynh đệ, nằm tạm ở đây nhé!
Dialogue: 0,1:59:57.50,2:00:04.00,Loithoai,,0000,0000,0000,,Viên Thuật diệt rồi,\NCông Tôn Toản diệt rồi.
Dialogue: 0,2:00:12.28,2:00:17.15,Loithoai,,0000,0000,0000,,Còn có ở đây, ở kia nữa.
Dialogue: 0,2:00:34.13,2:00:36.80,Loithoai,,0000,0000,0000,,Còn có ở đây
Dialogue: 0,2:00:54.55,2:00:55.47,Loithoai,,0000,0000,0000,,Đến đúng lúc lắm!
Dialogue: 0,2:00:55.86,2:00:57.67,Loithoai,,0000,0000,0000,,Ta biết là huynh không bỏ mặc ta.
Dialogue: 0,2:00:58.15,2:00:59.72,Loithoai,,0000,0000,0000,,Tôi đã nói sẽ có ngày chúng ta hội ngộ mà.
Dialogue: 0,2:01:00.05,2:01:01.27,Loithoai,,0000,0000,0000,,Cam tướng quân đã tử trận rồi.
Dialogue: 0,2:01:03.18,2:01:04.65,Loithoai,,0000,0000,0000,,Huynh ấy chết không phải vô ích đâu.
Dialogue: 0,2:01:07.81,2:01:08.88,Loithoai,,0000,0000,0000,,Lửa sắp lan tới đây rồi.
Dialogue: 0,2:01:09.18,2:01:10.37,Loithoai,,0000,0000,0000,,Không còn thời gian đâu,\Nphải nhanh chóng cứu nhị tẩu.
Dialogue: 0,2:01:11.76,2:01:13.21,Loithoai,,0000,0000,0000,,Trước tiên phải tìm được\Ntên Tào Tháo đó đã.
Dialogue: 0,2:01:17.19,2:01:19.73,Loithoai,,0000,0000,0000,,Sở dĩ chúng ta bại trận này
Dialogue: 0,2:01:20.42,2:01:22.72,Loithoai,,0000,0000,0000,,chính là vì con đàn bà khốn kiếp đó.
Dialogue: 0,2:01:24.27,2:01:27.16,Loithoai,,0000,0000,0000,,Chính nó làm chúng ta\Nmất đi cơ hội tiến công.
Dialogue: 0,2:01:27.71,2:01:28.68,Loithoai,,0000,0000,0000,,Ta phải đi giết nó.
Dialogue: 0,2:01:28.93,2:01:29.78,Loithoai,,0000,0000,0000,,Giết nó đi!
Dialogue: 0,2:01:51.86,2:01:53.12,Loithoai,,0000,0000,0000,,Đừng hòng tiến thêm một bước.
Dialogue: 0,2:02:28.40,2:02:29.51,Loithoai,,0000,0000,0000,,Đừng để bọn địch xông qua.
Dialogue: 0,2:02:29.80,2:02:31.93,Loithoai,,0000,0000,0000,,Bên kia.\NMau!
Dialogue: 0,2:02:37.49,2:02:39.32,Loithoai,,0000,0000,0000,,Bảo vệ thừa tướng!\NGiết hết bọn chúng!
Dialogue: 0,2:02:39.67,2:02:40.49,Loithoai,,0000,0000,0000,,Thúc Tài.
Dialogue: 0,2:02:55.95,2:02:56.69,Loithoai,,0000,0000,0000,,Thúc Tài.
Dialogue: 0,2:03:02.03,2:03:03.83,Loithoai,,0000,0000,0000,,Tôi là Bàn Trư đây mà.
Dialogue: 0,2:03:07.00,2:03:08.22,Loithoai,,0000,0000,0000,,Tôi là Bàn Trư đây mà.
Dialogue: 0,2:03:09.37,2:03:10.76,Loithoai,,0000,0000,0000,,Bàn Trư?
Dialogue: 0,2:03:44.16,2:03:45.39,Loithoai,,0000,0000,0000,,Bàn Trư.
Dialogue: 0,2:03:48.85,2:03:50.61,Loithoai,,0000,0000,0000,,Vậy là cậu đã trở lại thăm tôi.
Dialogue: 0,2:03:54.46,2:03:56.22,Loithoai,,0000,0000,0000,,Tôi đã nói sẽ quay lại thăm cậu mà.
Dialogue: 0,2:03:56.70,2:03:57.91,Loithoai,,0000,0000,0000,,Bàn Trư.
Dialogue: 0,2:04:01.63,2:04:05.11,Loithoai,,0000,0000,0000,,Còn nhớ trò công kênh chứ?
Dialogue: 0,2:06:21.60,2:06:23.36,Loithoai,,0000,0000,0000,,Tao sẽ giết mày con đàn bà khốn kiếp!
Dialogue: 0,2:07:26.68,2:07:27.55,Loithoai,,0000,0000,0000,,Cấm động đậy!
Dialogue: 0,2:07:37.98,2:07:39.13,Loithoai,,0000,0000,0000,,Chu Du.
Dialogue: 0,2:07:39.83,2:07:41.14,Loithoai,,0000,0000,0000,,Chu đô đốc.
Dialogue: 0,2:07:41.56,2:07:42.17,Loithoai,,0000,0000,0000,,Tào tặc.
Dialogue: 0,2:07:42.19,2:07:43.08,Loithoai,,0000,0000,0000,,Im mồm!
Dialogue: 0,2:07:43.57,2:07:45.71,Loithoai,,0000,0000,0000,,Ngươi phải gọi là Tào thừa tướng.
Dialogue: 0,2:07:47.79,2:07:52.49,Loithoai,,0000,0000,0000,,Ta chẳng bao giờ nghĩ\Nmình sẽ bại vì một cơn gió.
Dialogue: 0,2:07:53.00,2:07:54.76,Loithoai,,0000,0000,0000,,Bởi vì ngươi chẳng hiểu thế nào là ý trời.
Dialogue: 0,2:07:55.22,2:07:59.79,Loithoai,,0000,0000,0000,,Ta cũng chẳng bao giờ nghĩ\Nmình sẽ bại vì một chén trà.
Dialogue: 0,2:08:02.41,2:08:05.32,Loithoai,,0000,0000,0000,,Với ngươi đương nhiên chiến tranh\Ncũng chỉ giống như một chén trà mà thôi.
Dialogue: 0,2:08:05.76,2:08:06.77,Loithoai,,0000,0000,0000,,Đúng là đáng tiếc!
Dialogue: 0,2:08:07.10,2:08:08.78,Loithoai,,0000,0000,0000,,Thực chất ngươi chẳng hề biết\Nmình chiến đấu vì cái gì
Dialogue: 0,2:08:09.10,2:08:10.80,Loithoai,,0000,0000,0000,,Ta phụng chiếu chỉ của Thiên tử
Dialogue: 0,2:08:11.13,2:08:14.63,Loithoai,,0000,0000,0000,,diệt trừ đám phản nghịch các ngươi.
Dialogue: 0,2:08:15.09,2:08:17.23,Loithoai,,0000,0000,0000,,Bọn ta không hề phản Thiên tử
Dialogue: 0,2:08:17.61,2:08:18.62,Loithoai,,0000,0000,0000,,mà là phản ngươi.
Dialogue: 0,2:08:18.89,2:08:19.87,Loithoai,,0000,0000,0000,,Ngông cuồng!
Dialogue: 0,2:08:56.59,2:08:57.78,Loithoai,,0000,0000,0000,,Oắt con.
Dialogue: 0,2:08:58.36,2:09:00.89,Loithoai,,0000,0000,0000,,Chuyến đi săn này thật thú vị!
Dialogue: 0,2:09:02.05,2:09:03.39,Loithoai,,0000,0000,0000,,Ngươi muốn săn gì nào?
Dialogue: 0,2:09:04.21,2:09:05.37,Loithoai,,0000,0000,0000,,Săn hổ.
Dialogue: 0,2:09:06.09,2:09:08.10,Loithoai,,0000,0000,0000,,Vừa trông thấy ngươi,
Dialogue: 0,2:09:09.38,2:09:12.29,Loithoai,,0000,0000,0000,,ngươi so với cha ngươi, còn kém rất xa.
Dialogue: 0,2:09:17.18,2:09:18.00,Loithoai,,0000,0000,0000,,Nào!
Dialogue: 0,2:09:28.68,2:09:30.65,Loithoai,,0000,0000,0000,,Ta là thừa tướng đương triều.
Dialogue: 0,2:09:31.07,2:09:33.84,Loithoai,,0000,0000,0000,,Các ngươi đều là phản tặc,\Nchết hết cũng chẳng có gì đáng tiếc.
Dialogue: 0,2:09:34.59,2:09:36.10,Loithoai,,0000,0000,0000,,Nếu như các ngươi\Nnguyện trung thành với ta
Dialogue: 0,2:09:36.68,2:09:37.70,Loithoai,,0000,0000,0000,,ta có thể miễn cho các ngươi tội chết.
Dialogue: 0,2:09:42.34,2:09:44.72,Loithoai,,0000,0000,0000,,Biết điều thì mau quỳ hết xuống!
Dialogue: 0,2:09:49.06,2:09:50.77,Loithoai,,0000,0000,0000,,Ngươi hết thời rồi.
Dialogue: 0,2:10:03.53,2:10:05.54,Loithoai,,0000,0000,0000,,Mau bỏ kiếm xuống.
Dialogue: 0,2:10:16.52,2:10:18.07,Loithoai,,0000,0000,0000,,Thừa tướng, giết hết bọn chúng đi.
Dialogue: 0,2:10:18.44,2:10:20.09,Loithoai,,0000,0000,0000,,Bọn chúng đều là một lũ đáng chết.
Dialogue: 0,2:10:22.45,2:10:23.65,Loithoai,,0000,0000,0000,,Tiểu Kiều.
Dialogue: 0,2:10:24.37,2:10:26.12,Loithoai,,0000,0000,0000,,Cả hai đều bình an cả chứ?
Dialogue: 0,2:10:26.66,2:10:28.62,Loithoai,,0000,0000,0000,,Bọn thiếp đều không sao cả.
Dialogue: 0,2:10:32.54,2:10:34.01,Loithoai,,0000,0000,0000,,Bọn ta bại trận rồi
Dialogue: 0,2:10:34.90,2:10:36.36,Loithoai,,0000,0000,0000,,đều vì mày mà ra cả.
Dialogue: 0,2:10:37.43,2:10:39.35,Loithoai,,0000,0000,0000,,Còn không chịu quỳ xuống.
Dialogue: 0,2:10:49.28,2:10:52.06,Loithoai,,0000,0000,0000,,Chỉ cần ngươi quỳ trước mặt ta ngay\Nlập tức, ta có thể không giết Tiểu Kiều.
Dialogue: 0,2:10:52.60,2:10:53.99,Loithoai,,0000,0000,0000,,Bọn ta sẽ không làm\Ntổn hại tới nàng ấy đâu.
Dialogue: 0,2:10:54.35,2:10:55.54,Loithoai,,0000,0000,0000,,"Bọn ta"?
Dialogue: 0,2:10:56.25,2:10:58.20,Loithoai,,0000,0000,0000,,Ngươi định nói bọn chúng ư?
Dialogue: 0,2:10:58.73,2:11:00.42,Loithoai,,0000,0000,0000,,Một lũ ô hợp này sao?
Dialogue: 0,2:11:01.11,2:11:02.38,Loithoai,,0000,0000,0000,,Bọn họ đều là bằng hữu của ta.
Dialogue: 0,2:11:04.86,2:11:06.14,Loithoai,,0000,0000,0000,,Trên chiến trường thì\Nlàm quái gì có bằng hữu.
Dialogue: 0,2:11:06.69,2:11:10.41,Loithoai,,0000,0000,0000,,Ngay ngày mai bọn chúng\Nsẽ là kẻ địch của ngươi thôi.
Dialogue: 0,2:11:14.76,2:11:16.09,Loithoai,,0000,0000,0000,,Hạ Hầu Đôn.
Dialogue: 0,2:11:16.59,2:11:17.56,Loithoai,,0000,0000,0000,,Đẩy ả ta xuống!
Dialogue: 0,2:11:17.85,2:11:18.67,Loithoai,,0000,0000,0000,,Đi mà chết đi!
Dialogue: 0,2:11:36.94,2:11:37.85,Loithoai,,0000,0000,0000,,Đô đốc.
Dialogue: 0,2:11:56.32,2:11:58.21,Loithoai,,0000,0000,0000,,Cả hai đều ổn chứ?
Dialogue: 0,2:12:17.76,2:12:18.68,Loithoai,,0000,0000,0000,,Chúng ta đi thôi.
Dialogue: 0,2:12:21.39,2:12:25.01,Loithoai,,0000,0000,0000,,Tôi nhất định sẽ đưa cậu về nhà.
Dialogue: 0,2:13:14.99,2:13:16.19,Loithoai,,0000,0000,0000,,Ngươi đi đi!
Dialogue: 0,2:13:17.35,2:13:20.08,Loithoai,,0000,0000,0000,,Ngươi từ đâu tới thì hãy về nơi đó.
Dialogue: 0,2:13:47.21,2:13:49.71,Loithoai,,0000,0000,0000,,Mọi người chẳng có ai chiến thắng cả.
Dialogue: 0,2:14:37.71,2:14:38.83,Loithoai,,0000,0000,0000,,Minh Minh.
Dialogue: 0,2:14:40.34,2:14:42.88,Loithoai,,0000,0000,0000,,Đi đi.\NĐi đi nào!
Dialogue: 0,2:14:54.94,2:14:56.08,Loithoai,,0000,0000,0000,,Minh Minh.
Dialogue: 0,2:15:06.66,2:15:08.66,Loithoai,,0000,0000,0000,,Phân lâu tất hợp, hợp lâu tất phân.
Dialogue: 0,2:15:09.49,2:15:11.72,Loithoai,,0000,0000,0000,,Tôi thấy Lưu Bị sẽ không chịu\Nxếp dưới người khác lâu đâu.
Dialogue: 0,2:15:12.11,2:15:14.96,Loithoai,,0000,0000,0000,,Tôi và huynh ắt hẳn sẽ có một ngày\Ngặp lại nhau trên chiến trường.
Dialogue: 0,2:15:15.63,2:15:18.84,Loithoai,,0000,0000,0000,,Chuyện xảy ra trong tương lai\Nchẳng ai có thể nói chuẩn xác được cả.
Dialogue: 0,2:15:19.53,2:15:21.75,Loithoai,,0000,0000,0000,,Thiên hạ anh hùng có rất nhiều.
Dialogue: 0,2:15:21.89,2:15:24.77,Loithoai,,0000,0000,0000,,Nhưng anh hùng mà tôi không muốn\Nđối đầu nhất, chính là huynh đó.
Dialogue: 0,2:15:25.98,2:15:28.25,Loithoai,,0000,0000,0000,,Huynh mới là đối thủ khiến tôi sợ nhất.
Dialogue: 0,2:15:28.59,2:15:29.19,Loithoai,,0000,0000,0000,,Tôi?
Dialogue: 0,2:15:29.56,2:15:32.49,Loithoai,,0000,0000,0000,,Tôi vẫn muốn về nhà ngủ một giấc cho đã.
Dialogue: 0,2:15:32.92,2:15:34.36,Loithoai,,0000,0000,0000,,Chắc hẳn huynh là một nông phu giỏi?
Dialogue: 0,2:15:34.66,2:15:36.96,Loithoai,,0000,0000,0000,,Thú thực tôi có trồng một vài cây hoa sen.
Dialogue: 0,2:15:37.26,2:15:38.67,Loithoai,,0000,0000,0000,,nhưng chẳng cây nào sống lâu cả.
Dialogue: 0,2:15:38.99,2:15:42.03,Loithoai,,0000,0000,0000,,Không cần phải khiêm tốn như vậy.\NHuynh xứng đáng được gọi là kỳ tài mà.
Dialogue: 0,2:15:44.06,2:15:47.38,Loithoai,,0000,0000,0000,,Trong lần liên minh này, tôi đã tìm được\Nnhững người bạn cùng vào sinh ra tử.
Dialogue: 0,2:15:48.17,2:15:49.81,Loithoai,,0000,0000,0000,,Không cần biết sau này\Nmọi chuyện thành ra thế nào.
Dialogue: 0,2:15:50.21,2:15:51.70,Loithoai,,0000,0000,0000,,Chu Du này sẽ luôn khắc cốt ghi tâm.
Dialogue: 0,2:15:52.05,2:15:54.14,Loithoai,,0000,0000,0000,,Lượng tôi cũng sẽ không bao giờ quên.
Dialogue: 0,2:16:01.31,2:16:02.19,Loithoai,,0000,0000,0000,,Khổng Minh.
Dialogue: 0,2:16:02.51,2:16:05.54,Loithoai,,0000,0000,0000,,Khi nào Minh Minh lớn lên, không được\Nkhiến nó phải ra chiến trường đâu đấy.
Dialogue: 0,2:16:05.91,2:16:07.01,Loithoai,,0000,0000,0000,,Yên tâm.
Dialogue: 0,2:16:07.40,2:16:09.32,Loithoai,,0000,0000,0000,,Sinh mệnh của nó là do tôi đem đến,
Dialogue: 0,2:16:09.92,2:16:12.07,Loithoai,,0000,0000,0000,,tôi sẽ hết lòng thương yêu nó.
